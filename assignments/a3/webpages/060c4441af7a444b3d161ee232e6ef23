<a href="{{thing.url}}" data-ng-click="onclick($event,$index)" rel="nofollow">
<img data-ng-src="{{thing.imageurl[0]}}" data-ng-alt="{{thing.title}}" width="161" height="73" data-ng-class="{ 'defaultImg' : thing.imageurl[0] === undefined || !thing.imageurl[0]}"/>
<span data-ng-class="{'progress-bar':storyViewIndex == ($index+1)}"></span>
<span class="three-things-order antenna" data-ng-bind="{{$index+1}}" data-ng-class="{'active':storyViewIndex == ($index+1)}"></span>
<figcaption class="thumbnail-headline antenna-medium elipsis" data-ng-class="{'active-thumbnail-headline':storyViewIndex == ($index+1)}" data-ng-bind="thing.title"></figcaption>
</a>
</figure>
</div>
</div>
</body>
