"use strict";/**
	 * ArgmentedEvent will hold some global information, such like window scroll postion,
	 * so that those information is only calculated once.
	 * @param {Object} option - The option for SyntheticEvent
	 */
function ArgmentedEvent(option){option=option||{},this.type=option.type||"",this.scroll=scroll,this.resize=resize}var scroll={delta:0,top:0},resize={width:0,height:0};module.exports=ArgmentedEvent},/* 25 */
