                    var pollAnswer = $("#poll-9899d25a-b874-11e5-bfae-937a816053cb-form input[name=answer]:checked").val();
                    var pollAnswerNice = $("#poll-9899d25a-b874-11e5-bfae-937a816053cb-form input[name=answer]:checked").attr('title');
                    var pollDmpData = 'Poll : Which SEC team has chances at a national championship next season? : ' + pollAnswerNice;
                    
                    if(!pollAnswer){
                        return false;
                    }
                    $.ajax({
                        type: 'POST',
                        url: '/sports/which-sec-team-has-chances-at-a-national-championship-next/poll_9899d25a-b874-11e5-bfae-937a816053cb.html',
                        data: 'format=json&action=poll:vote&answer='+pollAnswer,
                        success: function(result){
