<script type="text/javascript" src="http://extras.denverpost.com/media/js/slick/slick.min.js"></script>
<link rel="stylesheet" type="text/css" href="http://fonts.googleapis.com/css?family=Open+Sans:600" />

<div id="dp-top-stories-bar-source" style="display:none;">
    <script type="text/javascript" src="http://extras.denverpost.com/cache/merc_top_stories_bar.js"></script>
