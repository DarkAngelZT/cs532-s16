<script>(function(w, d){var executeScript = function() {var script = d.createElement("script");script.setAttribute("src", "http://g-ec2.images-amazon.com/images/G/01/AmazonBrandStoresPlatform/placements/showADR._V318447700_.js");var loadSlot = function(){showAdSlot("amsDetailRight", "ams-detail-right", "http://aax-us-east.amazon-adsystem.com/x/getad?pa=B01464D2QO&c=100&pc=3375251&pt=Detail&u=http%3A%2F%2Fwww.amazon.com&src=501&sz=245x135&ad-sid=0101bde5dfb929c36eecb5682ecd0ccd648961095776dfc5925ff70b63bfbcbd3e6e&pj=%7B%22asins%22%3A%22B01464D2QO%22%2C%22st%22%3A%22amzn.us.dp.sports%22%2C%22prid%22%3A%220101d35b705edcf062f4800bd8ac3a603f7375e3bb2ee0dbfe80a6426a44bb5dc1c3%22%2C%22bn%22%3A%223375251%22%7D&slot=ams-detail-right", 135, 245, 165, 4543, "DAadr", "http://z-ecx.images-amazon.com/images/G/01/browser-scripts/DA-us/DA-us-145031240._CB285399696_.js", "/aan/2009-09-09/ad/feedback.us/default?pt=RemoteContent&slot=main&pt2=us-external", "site=amazon.us;pt=Detail;slot=ams-detail-right;pid=B01464D2QO;asin=B01464D2QO;bn=3375251;prid=17WYBPJG5MXWMA9NRF0G;arid=68876a09daee4ac7899909498913ee8b;ad-sid=0101bde5dfb929c36eecb5682ecd0ccd648961095776dfc5925ff70b63bfbcbd3e6e", "margin-bottom:-7px;", "na", "", "", "");if (w.removeEventListener) {w.removeEventListener("show-ams-detail-right", loadSlot);}};var ieLoadSlot = function(e){if (e.propertyName === "show-ams-detail-right") {loadSlot();w.detachEvent("onpropertychange", ieLoadSlot);}};if (w.addEventListener) {w.addEventListener("show-ams-detail-right", loadSlot, false);} else {w.attachEvent("onpropertychange", ieLoadSlot);}d.getElementsByTagName('head')[0].appendChild(script);};if (w.P && w.P.AUI_BUILD_DATE) {P.when('cf', 'A').execute(executeScript); }else if (typeof(amznJQ) !== "undefined") {amznJQ.onCompletion('amznJQ.criticalFeature', executeScript);}})(window, document);</script>
