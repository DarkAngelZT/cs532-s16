            jQuery(document).ready(function($){
                var gallery_ad_74b668e8_cc84_11e5_bb51_c3625b9868a2_photo_modal = false;
                var gallery_changed_74b668e8_cc84_11e5_bb51_c3625b9868a2_photo_modal = false;
                $('#gallery-items-74b668e8-cc84-11e5-bb51-c3625b9868a2-photo-modal').owlCarousel({
                    items: 1,
                    autoHeight: true,
                    lazyLoad: true,
                    loop: false,
                    navRewind: true,
                    responsiveRefreshRate: 500,
                    
                    
                    dots : false,
                    onChanged: function(){
                        
                        

                        
                            var currItem = $('#gallery-items-74b668e8-cc84-11e5-bb51-c3625b9868a2-photo-modal .photo-container').eq(this._current).data('count');
                            if(currItem){
                                $('#carousel-74b668e8-cc84-11e5-bb51-c3625b9868a2-photo-modal .current-count').html(currItem);
                            }
                            $('.modal.in, #carousel-74b668e8-cc84-11e5-bb51-c3625b9868a2-photo-modal .item').removeClass('hover');
                        
                    },
                    onTranslated:function(){
                        
                        
                        
                            __tnt.trackEvent($('#gallery-items-74b668e8-cc84-11e5-bb51-c3625b9868a2-photo-modal .photo-container').eq(this._current).data('track-object'));
                        

                        
                        
                    }
                });

                
                var photo_carousel_74b668e8_cc84_11e5_bb51_c3625b9868a2_photo_modal = $('#gallery-items-74b668e8-cc84-11e5-bb51-c3625b9868a2-photo-modal').data('owlCarousel');

                $('#carousel-74b668e8-cc84-11e5-bb51-c3625b9868a2-photo-modal .photo-carousel-control.left').on(__tnt.client.clickEvent, function(e){
                    photo_carousel_74b668e8_cc84_11e5_bb51_c3625b9868a2_photo_modal.prev();
                });

                $('#carousel-74b668e8-cc84-11e5-bb51-c3625b9868a2-photo-modal .photo-carousel-control.right').on(__tnt.client.clickEvent, function(e){
                    photo_carousel_74b668e8_cc84_11e5_bb51_c3625b9868a2_photo_modal.next();
                });
                    
                    });
        </script>
