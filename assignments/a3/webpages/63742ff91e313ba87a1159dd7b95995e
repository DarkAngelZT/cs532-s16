	  $.getScript('http://api.youneeq.ca/scripts/json2.js'),
	  $.getScript('http://api.youneeq.ca/scripts/detect_timezone.js'),
	  $.getScript('http://api.youneeq.ca/scripts/popper/tinybox.js'),
	  $.getScript('http://api.youneeq.ca/scripts/popper/script.js'),
	  $.getScript('http://cdn.jsdelivr.net/jquery.dotdotdot/1.6.16/jquery.dotdotdot.min.js')
	).then(function() {
		$.ajaxSetup({
		  cache: false
		});
	   $.getScript('http://api.youneeq.ca/app/yqmin',function() {
			Yq.onready(my_yq_init);
		});
	});

   function on_yq_suggest(suggestions) {
      //check if there are suggested results, if so populate 'youneeq' div
      if (suggestions && suggestions.suggest && suggestions.suggest.node) {
         var nodeids = suggestions.suggest.node, sponsored = false, stories = '';

         // add sponsored content
         if (suggestions.sponsored_content && suggestions.sponsored_content.node
             && suggestions.sponsored_content.node[0]) {
            sponsored = suggestions.sponsored_content.node[0];
            nodeids = nodeids.filter(function(e) {
               return e.id != sponsored.id;
            });
            nodeids.splice(3, 0, sponsored);
            if (nodeids.length > 6)
               nodeids = nodeids.slice(0, 6);
         }

         //add links, and apply sidebar styles - this could also be handled with a list
         for (var i = 0; i < nodeids.length; i++) {
            var a_id = nodeids[i].id, a_title = nodeids[i].title, a_url = nodeids[i].url, a_img = nodeids[i].image,
                a_desc = nodeids[i].description, a_has_img = true,
                a_is_sponsored = nodeids[i].categories[0] == 'Sponsored',
                a_src = { href: nodeids[i].domain, name: nodeids[i].domain, css: 'other', ext: true,
                          def_img: 'http://www.elkharttruth.com/libercus/default/graphics/FBlogo.jpg' };

            switch (a_src.href) {
               case 'elkharttruth.com':
                  a_src.name = 'The Elkhart Truth';
                  a_src.css = 'elkt';
                  a_src.ext = false;
                  break;
               case 'flavor574.com':
                  a_src.name = 'Flavor 574';
                  a_src.css = 'f574';
                  a_src.def_img = 'http://www.flavor574.com/libercus/default/graphics/logosquare.jpg';
                  break;
               case 'thefanindiana.com':
                  a_src.name = 'The Fan Indiana';
                  a_src.css = 'fan';
                  break;
               case '953mnc.com':
                  a_src.name = '95.3 MNC';
                  a_src.css = 'mnc';
                  a_src.def_img = 'http://www.953mnc.com/.global/default/graphics/logosquare.jpg';
            }

            if (!a_img || a_img == 'http://www.elkharttruth.com/libercus/default/graphics/logosquare.jpg'
                       || a_img == 'http://m.elkharttruth.com/libercus/default/graphics/logosquare.jpg'
                       || a_img == 'http://www.953mnc.com/.global/default/graphics/logosquare.jpg'
                       || a_img == 'http://m.953mnc.com/.global/default/graphics/logosquare.jpg') {
               a_has_img = false;
               a_img = a_src.def_img;
            }

            stories += '<a class="yqe_article_card ' + a_src.css + (a_has_img ? ' yqe_has_image' : ' yqe_no_image')
                     + '" href="' + a_url + '"' + (a_src.ext ? ' target="blank"' : '') + ' title="' + a_desc
                     + '" data-yq-id="' + a_id + '" data-yq-title="' + a_title + '"><div class="yqe_img_box"><img '
                     + 'id="yqe_image_' + i + '" src="' + a_img + '" alt="' + a_desc + '" /></div><div style="height: '
                     + '6em;"><div ' + 'class="yqe_title_box">' + a_title + '</div><div class="yq_source'
                     + (a_is_sponsored ? ' yq_sponsored">Sponsored' : '">' + a_src.name) + '</div></div></a>';
         }

         $('#youneeq_holder').append(stories + '<div class="clear" style="height: 1px;">&nbsp;</div>');
         $('.yqe_article_card').each(function() {
            var yq_id = $(this).data('yqId'), yq_title = Yq.titleTrim($(this).data('yqTitle')),
                yq_url = $(this).attr('href');
            $(this).on('mousedown.yq', function() {
               Yq.yq_panel_click(yq_url, yq_title, yq_id);
            });
         });
         $('.yqe_has_image .yqe_img_box img').load(set_image_size);
         $('.yqe_title_box').dotdotdot();
      }
   }

   function set_image_size() {
      var par_styles = window.getComputedStyle(this.parentNode);
      var width = parseFloat(par_styles.width);
      var height = parseFloat(par_styles.height);
      var ratio = width / height;
      var temp = 0;
      var margin = 0;
      var image_width = this.naturalWidth;
      var image_height = this.naturalHeight;
      var image_ratio = image_width / image_height;

      if (ratio < image_ratio) {
         var temp = image_height / height;
         margin = Math.round((image_width / temp - width) / -2);
         this.style.width = image_width / temp + 'px';
         this.style.height = height + 'px';
         this.style.maxWidth = image_width / temp + 'px';
         this.style.marginLeft = margin + 'px';
         this.style.marginBottom = '0px';
      }
      else if (ratio > image_ratio) {
         var temp = image_width / width;
         margin = Math.round((image_height / temp - height) * -1);
         this.style.width = width + 'px';
         this.style.height = image_height / temp + 'px';
         this.style.maxWidth = width + 'px';
         this.style.marginLeft = '0px';
         this.style.marginBottom = margin + 'px';
      }
      else {
         this.style.width = width + 'px';
         this.style.height = height + 'px';
         this.style.marginLeft = '0px';
         this.style.marginBottom = '0px';
      }
   }
   
   function my_yq_init() {
      var ptitle = '', categories = [], content_id = '', description = '', image_link = '', pub_date = '',
          canon_href = '', user_obj;

      gigya.socialize.getUserInfo({
       callback:function(response){
           if (response.errorCode == 0)
           {
               user_obj=response['user'];
               Yq.observeMin({
                   'gigya': user_obj
               }, on_yq_suggest)
           }
           else
               user_obj = {};
         }
      });

      if ($('meta[property="og:type"]').attr('content') === "article") {
         //if the url contains 'news' it's an article page, if this is incorrect the 'og:type' tag appears to identify article pages consistently...
         //populate ptitle field, used with panel results option
         ptitle = "Columnists debate whether public medical records matter in a presidential race" /* INSERT TEASER TITLE HERE */; 
         //if title is not the correct content identifier then amend
         content_id = "20160205067" /* INSERT UNIQUE ID HERE */;
         //this should ideally be the name of the group(s) associated with the content, passed in as an array - e.g. ['news','living']
         categories = ['discussions'] /* INSERT CATEGORY HERE - AS ARRAY */;
         //article description
         description = $('meta[property="og:description"]').attr('content');
         //grab the image link
         image_link =  "http://www.elkharttruth.com/image/2016/02/05/200x200_b0/APTOPIX-DEM-2016-Debate-Sanders-Clinton-3.jpg";  /* INSERT IMAGE HERE */
         //publish_date
         pub_date = "2016-02-08" /* INSERT PUBLISH DATE HERE - ISO 8601 */;
         //canonical url
         canon_href = $('meta[property="og:url"]').attr('content');
      }

      //content or not content page logic
      if (ptitle.length > 0) { //if 'condition that's only true on content pages' then do this...                          
         Yq.observe({
            'domain': window.location.hostname, //domain name
            'content_type': (categories[0] == 'Sponsored' ? 'sponsored' : 'article'),
            'observe': [{
               'type': 'node',
               'name': content_id,
               'title': ptitle, //required for 'is_panel_builder'
               'categories': categories,
               'description': description,
               'image': image_link,
               'create_date': pub_date
            }],
            //fetches 6 recommendations that can come from any site running the observe observable, with no category restrictions and returning content_id, url and title
            'suggest': [{'type': 'node', 'count': 6, 'sponsored_count': 1, 'categories': [], 'is_panel_detailed': 'true', 'isAllClientDomains': 'true',
                         'panel_custom': ['title', 'url', 'description', 'image', 'domain', 'categories']}],
            'href': canon_href,
            'gigya': user_obj
         }, on_yq_suggest);
      }
      else { //if identifier not found (i.e. non-content page) do this...
         Yq.observe({
            'domain': window.location.hostname, //domain name
            //fetches 6 recommendations that can come from any site running the observe observable, with no category restrictions and returning content_id, url and title
            'suggest': [{'type': 'node', 'count': 6, 'sponsored_count': 1, 'categories': [], 'is_panel_detailed': 'true', 'isAllClientDomains': 'true',
                         'panel_custom': ['title', 'url', 'description', 'image', 'domain', 'categories']}],
         'gigya': user_obj
         }, on_yq_suggest);
      }
   }
});
</script>
					
				
							
				
		
			</div>

		</article>
