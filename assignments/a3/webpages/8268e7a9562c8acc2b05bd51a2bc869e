<script type="text/javascript">
	var discussionRightSideQTips = function(){
		$('.discuss-rep,.discussion-rep-state,.admin-approved-toggle').qtip({
			style: {
				classes: 'qtip-dark qtip-shadow larger-qtip-text',
				tip: {
					width: 12,
					height: 12
				}
			},
			position: {
				my: 'bottom right',
				at: 'top left'
			}
		});
	};
	$(document).ready(function(){
		var windowTitle =document.title;
		var startsWith = function(needle,haystack){
			return haystack.indexOf(needle) === 0;
		};
		var articleTwitterText = function(url,url2,id,author,elem){
			return encodeURIComponent('Check out '+author+'\'s comment on '+$('.game-text a').text().trim()+' '+url2);
		};
		var newsTwitterText = function(url,url2,id,author,elem){
			return encodeURIComponent('Check out '+author+'\'s comment on '+$('.news-page-main-title').text().trim()+' '+url2);
		};
		var hardwareTwitterText = function(url,url2,id,author,elem){
			return encodeURIComponent('Check out '+author+'\'s comment on '+$('#art_g_title').text().trim()+' discussion '+url2);
		};
		var fallbackTwitterText = function(url,url2,id,author,elem){
			return encodeURIComponent('Check out '+author+'\'s comment on '+windowTitle+' discussion '+url2);
		};
		$('.share-anchor-twitter').click(function(e){
			var elem = $(this);
			e.preventDefault();
			var parser = document.createElement('a');
			parser.href = document.URL;
			var url = elem.data('discussion-social-url');
			var url2 = url.replace(/ /g,'%20');
			var id = elem.data('discussion-social-id');
			url2 ='http://www.game-debate.com/viewComment/'+id;
			$.post('/recordDiscussionLink/'+id,{url:"http%3A%2F%2Fwww.game-debate.com%2Fnews%2F%3Fnews%3D19345%26game%3DNone%26title%3DIntel%2520VP%2520Calls%2520Effective%2520End%2520To%2520Moores%2520Law%2520With%2520Focus%2520On%2520Efficiency%2520Over%2520Performance"},function(){});
			var author = elem.data('discussion-social-author');
			var twitterBegin = 'https://twitter.com/intent/tweet?text=';
			var twitterUrl = '';
			if(startsWith('/articles',parser.pathname)){
				twitterUrl = articleTwitterText(url,url2,id,author,$(this));
			}else if(startsWith('/news',parser.pathname)){
				twitterUrl = newsTwitterText(url,url2,id,author,$(this));
			}else if(false){
				twitterUrl = hardwareTwitterText(url,url2,id,author,$(this));
			}else{
				twitterUrl = fallbackTwitterText(url,url2,id,author,$(this));
			}
			twitterUrl = twitterBegin+twitterUrl;
			window.open(twitterUrl);
		});
		discussionRightSideQTips();
		$('.qtip-discussion-down-buttons').qtip({
			style: {
				classes: 'qtip-dark qtip-shadow larger-qtip-text',
				tip: {
					width: 12,
					height: 12
				}
			},
			position: {
				my: 'top left',
				at: 'bottom center'
			},
			hide: {
				fixed: true,
				delay: 1000
			},
			events:{
				show: function(event, api){
					$('.qtip-discussion-down-buttons').qtip('hide');
				}
			}
		});
		$('.share-anchor-link').click(function(e){
			e.preventDefault()
			var that = $(this);
			$('<div />').html('<input id="textarea-'+that.data('discuss-id')+'" value="'+that.attr('href')+'">').dialog({
				dialogClass: 'discuss-modal-copy',
				closeText:'x',
				width:200,
				height:50,
				open: function( event, ui ) {
					$('#textarea-'+that.data('discuss-id')).focus().select();
				}
			});
			$.post('/recordDiscussionLink/'+that.data('discuss-id'),{url:"http%3A%2F%2Fwww.game-debate.com%2Fnews%2F%3Fnews%3D19345%26game%3DNone%26title%3DIntel%2520VP%2520Calls%2520Effective%2520End%2520To%2520Moores%2520Law%2520With%2520Focus%2520On%2520Efficiency%2520Over%2520Performance"},function(){});
			return false;
		});

		$('.qtip-share-icons').qtip({
			style: {
				classes: 'qtip-dark qtip-shadow larger-qtip-text',
				tip: true
			},
			show: {
				delay: 0
			},
			position: {
				my: 'bottom right',
				at: 'top left'
			}
		});

	});
	$(function(){
		elem = window.location.hash;
		if(elem && elem.indexOf('discuss_')!= -1){
			elem = elem.slice(1);
			if (elem != 'discussForm'){
				$('[name='+elem+']').parent().effect('highlight',{},5000);
			}
		}
		$('.rig-verified').mouseenter(function(){
			var floater = $('.bench-floater',this);
			var m_id = floater.attr('data-mid');
			var r_id = floater.attr('data-rid');
			var isOpen = $('.bench-floater',this).attr('data-isOpen');
			if (isOpen == 0){
				floater.load('/global/benchList.php',{m_id:m_id,r_id:r_id},function(){
					floater.css('display','block').css('z-index',1000).attr('data-isOpen',1);
				});
			}else{
				floater.css('display','block');
			}
		});
		$('.rig-verified').mouseleave(function(){
			$('.bench-floater',this).css('display','none').css('z-index',0);
		});
		$('a.rig-verified').click(function(){
			var m_username = $('.bench-floater',this).attr('data-musername');
			window.open('/members/index.php?member='+m_username+'#bench','_self');
		});
	});
</script>
