        $("#login-box-form-page-manager-pop").addClass("openid-form-processing blox-loading");
        $(".register-column, .login-column-left, .login-column-right, a#login-box-create-link-page-manager-pop").hide();
    });
// style buttons
    $(".jquery-ui-button").button()

// validate form
