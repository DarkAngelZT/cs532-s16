  <script>
    // Last chance to execute JS before
// DTM _satellite logs page view

require(['App', 'dtm'], function(App, satellite){

  // Helper to get all elements with given attribute
  function getAllElementsWithAttribute(attribute) {
    var matchingElements = [];
    var allElements = document.getElementsByTagName('*');
    for (var i = 0, n = allElements.length; i < n; i++) {
      if (allElements[i].getAttribute(attribute)) matchingElements.push(allElements[i]);
    }
    return matchingElements;
  }

  // Get all product log elements (listings) and
  // store data-product-log string to VPAPP global
  var productsSelector = 'data-product-log';
  var products = getAllElementsWithAttribute(productsSelector);
  
  App.productsChunkCount = 10;
  App.productViewsChunk = 0;

  for (var i = 0; i < products.length; i++) {
    App.results.productViews.push( products[i].getAttribute(productsSelector) )
  }

  for (var k = 0; k < App.results.productViews.length; k++) {
    if (k % App.productsChunkCount === 0) {
      _satellite.track('productViewsLogChunk')
    }
  }
  
});

</script>

