	function moveScroller2() {
	    var move2 = function() {
	        var s2 = $("#share");
	        var st2 = $(window).scrollTop();
	        var ot2 = $("#fixsharetop").offset().top - 105;
     		var bt2 = $("#fixsharebottom").offset().top - s2.height() - 150;
	
	        if(st2 > ot2) {
	            s2.css({
	                position: "fixed",
	                top: "105px",
	            });	
	        } 
	
			if(st2 > bt2 ) {
	            s2.css({
	                position: "absolute",
				top: bt2 - (120 + $(".blt").height())
	            });
	        }
	
			if(st2 < ot2) {
	            s2.css({
	                position: "relative",
	                top: "0px",
	            });
	       	}
	
	    };
	    $(window).scroll(move2);
	    move2();
	}
	$(document).ready(function(){
		moveScroller2();
	});
	</script>
