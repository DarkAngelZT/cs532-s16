<script type="text/javascript">
    

var count = 0;

function clearDefault(el) {
    if (el.value == "Type your comment here") el.value = ""
    xdelay();
}

function setDefault(el) {
    if (el.value == "") el.value = "Type your comment here"
    xdelay();
}

function xcount() {
    var z = document.getElementById("count_box");
    var t = document.getElementById("ContentPlaceHolder1_ctrl_prop_details_new1_ctrl_comments1_txt_comment").value;

    if (t.length > 2000) {
        if (count == 0) {
            alert("A maximun of 2000 characters are allowed, please make your comment shorter.");
            count = count + 1;
            document.getElementById("ContentPlaceHolder1_ctrl_prop_details_new1_ctrl_comments1_btn_submit").disabled = true;
        }
    } else {
        if (t != "Type your comment here") {
                z.innerHTML = 2000 - t.length;
            }
            document.getElementById("ContentPlaceHolder1_ctrl_prop_details_new1_ctrl_comments1_btn_submit").disabled = false;
        count = 0;
    }
}

function xdelay() { setTimeout("xcount()", 200); }

function openWindow(sUrl) {
    window.open(sUrl, '', 'menubar=0,width=400,height=250'); return false;
}

function checkLoginState() {
    FB.getLoginStatus(function (response) {
        if (response.status === "connected") {
            location.reload();
        }
    });
}

</script>

