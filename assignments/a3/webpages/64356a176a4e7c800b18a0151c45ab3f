<script>
var sound = document.getElementById("sound");
function sectotime(totalSec) {
		var hours = parseInt( totalSec / 3600 ) % 24;
		var minutes = parseInt( totalSec / 60 ) % 60;
		var seconds = parseInt(totalSec % 60, 10);
		if (hours < 10) {
			hours = "0"+hours;
		}
		if (minutes < 10) {
			minutes = "0"+minutes;
		}
		if (seconds < 10) {
			seconds = "0"+seconds;
		}
		return minutes+":"+seconds;
	}
	$(function() {
		var nextclaim = ;
		var header = " time left- Robot Coin Game";
		var myVar = setInterval(function(){
			if (nextclaim <= 0) {
				clearInterval(myVar);

				if(document.getElementById('check').checked) {
        sound.play();}
				location.reload();


			}else{
			nextclaim--;
			$('#claimagain2').html(sectotime(nextclaim));
			$('#claimagain').html(sectotime(nextclaim));
			$('#claimagainheader').html(sectotime(nextclaim)+ header);
			}
		}, 1000);
		});
	</script>
<script>

	$(function() {

		$('input[name="percentage_donation"]').on("click",function() {
			var donation = ;
		var value =this.value;
		if (value == 'a'){
			var percentage = 1;
		}
		if (value == 'b'){
			var percentage = .5;
		}

		if (value == 'c'){
			var percentage = .25;
		}
			$("#totalSum_donation").html(Math.round((percentage* donation)).toLocaleString() + ' satoshis');
		});
		$('input[name="percentage_donation"]:checked').click();


		$('input[name="percentage_claim"]').on("click",function() {
		var donation = ;
		var value =this.value;
		if (value == 'a'){
			var percentage = 1;
		}
		if (value == 'b'){
			var percentage = .5;
		}

		if (value == 'c'){
			var percentage = .25;
		}
		$("#totalSum_claim").html(Math.round((percentage* donation)).toLocaleString() + ' satoshis');
	});
	$('input[name="percentage_claim"]:checked').click();
	});


	</script>
