                    itemprop="image"/>
            
        
                                        
                                        
                                </div><div class="caption-container">
                                        <button class="btn btn-link collapse fa fa-minus-circle pull-right photo-carousel-caption-toggle" data-toggle="collapse" data-instance="#carousel-8f638e11-7283-5d2a-aea7-e99da79f6bf6-photo-modal" data-target=".caption-inner-8f638e11-7283-5d2a-aea7-e99da79f6bf6-photo-modal"></button>
