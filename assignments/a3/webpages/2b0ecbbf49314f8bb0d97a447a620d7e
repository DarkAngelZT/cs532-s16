      <script type="text/javascript">
         var bsComplete=0;
         var sbComplete=0;
         var bidsExpired=0;
         var _adCallsFired = 0;
   
         window.load_bids = function (callback) {
            // The following code ensures that the callback method is run
            // after no more than 800 milliseconds
            setTimeout(function(){
               bidsExpired=1;
               callback();
               callback = function(){};
            }, 800);
   
            if (callback == null) callback = function () {};
            var node = document.getElementsByTagName("script")[0];
   
            // Beanstock Nucleus
            var bsm_nucleus = document.createElement("script");
            bsm_nucleus.async = true;
            bsm_nucleus.type = "text/javascript";
            var useSSL = "https:" == document.location.protocol;
            bsm_nucleus.id = 'cx-nuke';
            bsm_nucleus.src = (useSSL ? "https:" : "http:") + "//cdn.beanstock.com/pub/cx/flagTag.js";
   
            bsm_nucleus.setAttribute('data-bsm-pub', "cbsinteractive");
            bsm_nucleus.setAttribute('data-bsm-pub-zone', "cbssports.com/nucleus");
            bsm_nucleus.setAttribute('data-bsm-sizes', "728x90 300x250");
            bsm_nucleus.setAttribute('data-bsm-zone-map-url', "//cdn.beanstock.com/pub/cbsinteractive/zm.js");
            bsm_nucleus.setAttribute('data-bsm-event-host', "//helix.beanstock.co");
            bsm_nucleus.setAttribute('data-bsm-flag', "true");
            bsm_nucleus.setAttribute('data-bsm-version', "v3");
            bsm_nucleus.addEventListener("onNucleusComplete", function () {
               bsComplete=1;
               CBSi.log("bsComplete");
               callback();
            });
            node.parentNode.insertBefore(bsm_nucleus, node);
   
            
            // Begin Sonobi Trinity
            var associations = {
               'XFP_mpu_bottom' : 'bdcf493db8c2cb5e7c80','XFP_leader_plus2_bottom' : '9b1d4cf238b9b3cf3202'
            };
            var sbi_script = document.createElement('script');
            sbi_script.async = true;
            sbi_script.type = 'text/javascript';
            sbi_script.src = 'http://apex.go.sonobi.com/trinity.js?key_maker=' + JSON.stringify(associations) + '&s=' + Math.floor(Math.random() * 1000);

            if (sbi_script.readyState) {
               sbi_script.onreadystatechange = function () {
                  if (sbi_script.readyState == "loaded" || sbi_script.readyState == "complete") {
                     sbi_script.onreadystatechange = null;
                     sbComplete=1;
                     CBSi.log("sbComplete");
                     callback();
                  }
               };
            } else {
               sbi_script.onload = function () {
                  sbComplete=1;
                  CBSi.log("sbComplete");
                  callback();
               };
            }
            node.parentNode.insertBefore(sbi_script, node);
            // End Sonobi Trinity
   
         };
   
         // Function which will both add Sonobi variables to targeting and load ads.
         var load_ads = function(){
             CBSi.log("Calling load_ads.  bidsExpired="+bidsExpired+" bsComplete="+bsComplete+" sbComplete="+sbComplete);
             if(_adCallsFired) return;
             if(bidsExpired==1 || (bsComplete==1 && sbComplete==1)) {
                 _adCallsFired=1

                 
                 // Sonobi Conditional
                 // NOTE the Beanstock tag adds the targeting variables automatically
                 // add Sonobi variables to the ad targeting
                 if (sbComplete==1) {
                     googletag.cmd.push(function() {
                         googletag.pubads().setTargeting("sbi_dc", window.sbi_dc);  // Page level targeting for the sbi_dc key
                         // For each key in adSlots iterate over it and add the slot-level targeting.
                         for (var adSlotKey in adSlots) {
                             CBSi.log("SB: adSlotKey is :"+adSlotKey);
                             for (var key in window.sbi_trinity[adSlotKey]) {
                                 CBSi.log(" adding targeting key:"+key+" value:"+window.sbi_trinity[adSlotKey][key]);
                                 adSlots[adSlotKey].setTargeting(key, window.sbi_trinity[adSlotKey][key]);
                             }
                         }
                     });
                 }
                 // End Sonobi conditional
   

                 googletag.cmd.push(function() {
                     // Load the ads.
                     CBSi.log("Firing ads to refresh");
                     googletag.pubads().refresh();
                 });
             }
         };
 
         load_bids(load_ads);
         </script>
