<script>


$('div.sad').bind('inview', r_ads); 
$('div.sad728').bind('inview', r_ads728); // dynamic load 728x90 tags

function r_ads(event, visible)
{

    // this code is the old OPENX code
    //$(this).closest('.sad').load('/ads/openx-300x250-atf.html'); // will only load the ads on .sad 
    var g_num = 1 + Math.floor(Math.random() * 40); // generates random number for unique google ad.
    $(this).closest('.sad').attr('id', 'grad_'+g_num); // adds the id to the ad div that just came into view
    var slot = googletag.defineSlot('/33922401/GameRant_300x250_ATF_1', [300, 250], 'grad_'+g_num).addService(googletag.pubads());
    googletag.display('grad_'+g_num);
    googletag.pubads().refresh([slot]);   
    $(this).attr("class", "notinview");
    _gaq.push(['_trackEvent', 'Ad Views', '1', 'John Jacques']);
    console.log('300x250 DFP Ad Loaded');
}


function r_ads728(event, visible)
{
  var current_width = $(window).width();
  if(current_width > 400)
  {
    
    // this code is old OPENX code for 728x90
    //$(this).closest('.sad728').html('<iframe id="7d71ec4cc0" name="7d71ec4cc0" src="http://us-ads.openx.net/w/1.0/afr?auid=538034578&cb=INSERT_RANDOM_NUMBER_HERE" frameborder="0" scrolling="no" width="728" height="90"><a href="http://us-ads.openx.net/w/1.0/rc?cs=7d71ec4cc0&cb=INSERT_RANDOM_NUMBER_HERE" ><img src="http://us-ads.openx.net/w/1.0/ai?auid=538034578&cs=7d71ec4cc0&cb=INSERT_RANDOM_NUMBER_HERE" border="0" alt=""></a></iframe>');
    
    //DFP code for the 728x90
    var g_num_728 = 1 + Math.floor(Math.random() * 40); // generates random number for unique google ad.
    $(this).closest('.sad728').attr('id', 'grad_'+g_num_728); // adds the id to the ad div that just came into view
    var slot = googletag.defineSlot('/33922401/GameRant_728x90_ATF_1', [728, 90], 'grad_'+g_num_728).addService(googletag.pubads());
    googletag.display('grad_'+g_num_728);
    googletag.pubads().refresh([slot]);   
    $(this).attr("class", "notinview");
    _gaq.push(['_trackEvent', 'Ad Views', '1', 'John Jacques']);
    console.log('728x90 DFP Loaded');
  } 
}


$('img[data-original]').bind('inview', r_images);
function r_images(event, visible)
{
  var $this = $(this);
  $this.attr("src", $this.attr("data-original"));
  $this.removeAttr("data-original");

}



function dynamic_urls() 
{
  $('.description-holder').waypoint(function(direction) 
  {
      if (direction === 'down') 
      {           
        var url = $(this).data('url');
        var title = $(this).attr('title');
        if(typeof title != 'undefined')  
        {
          
          var obj = { Title: title, Url: url }; // this needs tweaking but sets title and url
          history.pushState(obj, obj.Title, obj.Url); // this pushes the state to the 'page'
          window.document.title = title; 
          //console.log('Moving Down Loading Title: '+title);
          //event.stopPropagation(); // if I dont do this it loads titles for all description-holders.
          //waypoint.disable();
        }    
      }
  }, { offset: '150px' });

 $('.current_marker').waypoint(function(direction) 
  {
      if (direction === 'up') 
      {
      var url = $(this).data('url');
      var title = $(this).attr('title');
      
        var obj = { Title: title, Url: url }; // this needs tweaking but sets title and url
        history.pushState(obj, obj.Title, obj.Url); // this pushes the state to the 'page'
        window.document.title = title;
      
     
      }
  }, {
 
    });


}


    
//var entry = $(this).closest('.description-holder');
dynamic_urls();

$('.pager').bind('inview', monitor);

function monitor(event, visible)
{
  if(visible)
  {
    
    var url = $(this).closest('.pager').attr('href'); // gets post url
    var title = $(this).closest('.pager').attr('title'); // gets the title of the article
    var postid = $(this).closest('.pager').attr('postid');   // gets the post id
    var obj = { Title: title, Url: url }; // this needs tweaking but sets title and url
    jQuery(".pager").remove(); // remove marker that appends content so it doesnt go ape.   
    //console.log('Loading Next Article, Post ID = '+postid+' Title = '+title);
    
    // set a custom php function to call our content. 
    var ajaxpagination = {"ajaxurl":"http:\/\/gamerant.com\/wp-admin\/admin-ajax.php"};
    event.preventDefault();
  $.ajax({
    url: ajaxpagination.ajaxurl,
    type: 'post',
    data: {
      action: 'ridler_posts',
      post_id: postid
    },
    success: function( result ) {
           $( "#main" ).append(result); // add results to screen
           $('.pager').bind('inview', monitor); // this recalls the code so that it works. 
           $('img[data-original]').bind('inview', r_images); // this rebinds the code for lazy load
           $('div.sad').bind('inview', r_ads); 
           $('div.sad728').bind('inview',r_ads728);
           dynamic_urls();
            return false;


    }
  })

      
  }
}

// reset disqus comments on click of button.
$('.comments_button').on('click', function(){
  $('body .ridler_disqus').remove();
  $(this).closest('.comments_button').after('<div id="disqus_thread" class="ridler_disqus"></div>');
  var url = $(this).closest('.comments_button').attr('url');
    var postid = $(this).closest('.comments_button').attr('theid');
    var title = $(this).closest('.comments_button').attr('title');
  reset(postid, url, title);
});

</script>


<script type="text/javascript">
    /* * * CONFIGURATION VARIABLES: EDIT BEFORE PASTING INTO YOUR WEBPAGE * * */
    var disqus_shortname = 'gamerant';
    var disqus_url = '';
  var disqus_config = function () { 
    this.language = "en";
  };
    /* * * DON'T EDIT BELOW THIS LINE * * */
    (function() {
        var dsq = document.createElement('script'); dsq.type = 'text/javascript'; dsq.async = true;
        dsq.src = 'http://' + disqus_shortname + '.disqus.com/embed.js';
        (document.getElementsByTagName('head')[0] || document.getElementsByTagName('body')[0]).appendChild(dsq);
    })();
    /* * * Disqus Reset Function * * */
    var reset = function (newIdentifier, newUrl, newTitle, newLanguage) {
        DISQUS.reset({
            reload: true,
            config: function () {
                this.page.identifier = newIdentifier;
                this.page.url = newUrl;
                this.page.title = newTitle;
                this.language = newLanguage;
            }
        });
    };
</script>

<script>
$(window).scroll(function()
{
  var scroll = $(window).scrollTop();
  var width = $(window).width(), height = $(window).height();

  if (scroll >= 100 && width >= 400) 
  {
   var new_width = $('.wrap').width(); // get width of parent since I am going to change to FIXED.
   $("#header").width(new_width+20);
   $("#header").css({"position": "fixed", "z-index":"100", "top":"0px"});
  }
  if (scroll <= 99 && width >= 400) 
  {
       $("#header").css({"position": "", "z-index":"", "top":""});
  }
});

</script>

      


