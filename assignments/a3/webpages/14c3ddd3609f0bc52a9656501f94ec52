<script type="text/javascript">
 
 $(function(){
      showRecaptcha();
 });
   function showRecaptcha(element) {
     Recaptcha.create("6LfkOtESAAAAAE8UFtb6ds-qiA-7TL2HnGCXnqt2", "recaptcha_tags", {
       theme: "red",
       callback: Recaptcha.focus_response_field});
   }
