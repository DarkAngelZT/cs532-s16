		<script>
			$('.more_less').click(function() {
				if ($(this).attr('data-type') == "More") {
					$(this).hide();
					$(this).next().show();
					var t = $(this).parents('.search_result_content').find('ul>li').length;
					$(this).parents('.search_result_content').find('ul>li').slice(0, t).slideDown("fast");
				} else {
					$(this).hide();
					$(this).prev().show();
					var t = $(this).parents('.search_result_content').find('ul>li').length;
					$(this).parents('.search_result_content').find('ul>li').show().slice(3, t).slideUp("fast");
				}
			});

		</script>
		<script>
			$(function() {

				var tab_slider = function(slid) {
					$('#' + slid).flexslider({
						animation : "slide",
						slideshow : true,
						pauseOnHover : true,
						autoPlay : true,
						controlNav : true,
						animationLoop : true
					});
				}
				tab_slider('col-tab1-gallery');

				$(document).on('click', '.ui-tabs-anchor', function() {
					$(".list_content.list_full").css('height', 'auto');
					var info = $(this).attr('href').replace('#', '');
					var slide_id = info + '-gallery';
					tab_slider(slide_id);
				});
			});
