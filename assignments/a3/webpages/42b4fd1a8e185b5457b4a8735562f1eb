    <div class="span3"> 
     <script type="text/javascript">
$(function() {

    var loadDate = new Date();

    window.submitClicked = function(e,f){};

    function getWorkLog(evt, error, answer, callback, defaultTime, defaultObservations, isEdit) {

	if (isEdit == undefined) {
            isEdit = false;
        }

        if (!defaultTime) {
            var minutes = (new Date() - loadDate) / 1000 / 60;

            if (minutes < 1) {
                minutes = 1;
            }

            var hours = parseInt(minutes / 60);
            minutes = parseInt(minutes % 60);

            if (hours < 10) {
                hours = '0' + hours;
            }

            if (minutes < 10) {
                minutes = '0' + minutes;
            }

            defaultTime = hours + ':' + minutes;
        }

        if (!defaultObservations) {
            defaultObservations = "";
        }
        
        
        
		var chours = defaultTime.split(":");
		
		if(!isEdit && parseInt(chours[0]) < 1)
		{
                
                data = Object();
                data.logTime = defaultTime;
                data.markAnswered = "on";
                data.observations = "";
            
                callback(data.logTime, data.observations, data.markAnswered);
                //close();		
        }
		else
		{
	        var $dialog = commandUtils.showDialog({
	            nodes: commandUtils.renderTemplate('time-log-post-dialog', {isError: error, isAnswer: answer, time: defaultTime, observations: defaultObservations}),
	            extra_class: 'prompt',
	            beforeShow: function($dialog) {
	                $dialog.find('.logTime').timepicker({
	                    showPeriodLabels: false,
	                    hourText: 'Hours',
	                    minuteText: 'Minutes'
	                });
	
	                $dialog.find('.time-spent').click(function() {
	                    $dialog.find('.time-spent').removeClass('active');
	                    $(this).addClass('active');
	                    $dialog.find('.dialog-yes').removeAttr('disabled');
	
	                    if (!$(this).is('span')) {
	                        $dialog.find('.logTime').val($(this).attr('value'));
	                    }
	
	                    return false;
	                });
	
	                $dialog.find('.dialog-yes').attr('disabled', 'disabled');
	            },
	            yes_callback: function($dialog, close) {
	                var data = commandUtils.extractFormData($dialog);
	
	                var split = /(\d\d):(\d\d)/.exec(data.logTime);
	
	                if (!split) {
	                    $dialog.find('.logTime').css('border-color', 'red');
	                } else {
	
	                    function done() {
	                        callback(data.logTime, data.observations, data.markAnswered);
	                        close();
	                    }
	
	                    if (split[1] >= 1) {
	                        var msg = 'Are you sure you want to log {time}?'.replace('{time}', data.logTime);
	                        commandUtils.showConfirmation(evt, 'Please confirm', msg, done);
	                    } else {
	                        done();
	                    }
	                }
	            },
	            event: evt,
	            show_no: true
	        });
		}

    }

    window.getWorkLog = getWorkLog;

    var confirmIsAnswerMessages = {
        'description': 'Post as:',
        'pleaseSelect': 'Select one',
        'asAnswer': 'Answer to the question',
        'commentOnQuestion': 'Comment on the question',
        'commentOnAnswer': 'Comment on the answer by {author}, {date}',
        'commentOnComment': 'Comment on the comment by {author}, {date}'
        
        
    };
	
	
	function getObjectsChildren(parPost, data, level)
	{
		var toReturn = new Array();
		$.each(data,function(i,post){
			if(post.parentId == parPost.id)
			{
				post.summary = Array(level+1).join("-") + " " + post.summary;
				toReturn[toReturn.length] = post
       			var objects_children = getObjectsChildren(post,data,level+1);
       			if(objects_children.length > 0)
       			{
       				for(ii=0;ii<objects_children.length;ii++)
       				{
       					toReturn[toReturn.length] = objects_children[ii];
       				}
       			}
			}
		});
		return toReturn;
	}
	
	function getObjectDataParent(object,data)
	{
		var thisReturn;
		$.each(data,function(i,post){
			if(object.parentId == post.id)
			{
   				thisReturn =  object;
   				return false;
			}
		});
		return thisReturn;
	}

    var possibleLocations = false;

    function confirmIsAnswer(e, isAnswerCallback) {
        if (possibleLocations && $('#is-answer-selector').length) {
            return;
        } else  if (possibleLocations == 'fetching') {
            return;
        }

        try {
            if (/\/edit\.html$/.test(window.location)) {
                isAnswerCallback();
                return;
            }

            function showDialog(data) {
                possibleLocations = data;

                $.each(data.result.parentCandidates, function(i, post) {
                    if (post.type == "question") {
                        post.summary = confirmIsAnswerMessages.commentOnQuestion
                    } else {
                        var node = getObjectDataParent(post, data.result.parentCandidates);
                        if(node != null && node.type === "comment")
                        {
                            post.summary = confirmIsAnswerMessages.commentOnComment
                                    .replace('{author}', post.author.username)
                                    .replace('{date}', commandUtils.friendlyDate(new Date(post.creationDate)));
                        }
                        else
                        {
                            post.summary = confirmIsAnswerMessages.commentOnAnswer
                                    .replace('{author}', post.author.username)
                                    .replace('{date}', commandUtils.friendlyDate(new Date(post.creationDate)));
                        }
                    }
                });


                var newresults = new Array();
                var theQuestion;
                //first get the question and it's comments
                $.each(data.result.parentCandidates, function(i, post) {
                    if(post.type == "question")
                    {
                        newresults[newresults.length] = post;
                        theQuestion = post;
                        return false;
                    }
                });

                //first get the question and it's comments
                $.each(data.result.parentCandidates, function(i, post) {
                    if(post.parentId == theQuestion.id)
                    {
                        post.summary = "- " + post.summary;
                        newresults[newresults.length] = post;
                        var objects_children = getObjectsChildren(post,data.result.parentCandidates,2);
                        if(objects_children.length > 0)
                        {
                            for(ii=0;ii<objects_children.length;ii++)
                            {
                                newresults[newresults.length] = objects_children[ii];
                            }
                        }
                    }
                });

                data.result.parentCandidates = newresults;



                $.extend(data.result, confirmIsAnswerMessages);

                commandUtils.showDialog({
                    nodes: commandUtils.renderTemplate('confirm-is-answer-dialog', data.result),
                    'extra_class': 'prompt',
                    'yes_callback': function($dialog, close) {
                        var value = $dialog.find('select[name=is-answer]').val();

                        if (value == 'select') {
                            return false;
                        } if (value == 'answer') {
                            confirmAnswerOpen = false;
                            close();
                            isAnswerCallback();


                        } else {
                            var text = $('#body').val();
                            if($('#comment-'+value+'-info').length > 0)
                            {

                                $('#comment-'+value+'-info').find(".comment-reply").click();
                                $(".comment-form-container").find("[name='body']").val(text);
                                window.onbeforeunload = null;
                                window.location = '#comment-' + value + '-form';
                            }
                            else
                            {
                                $('#answer-container-' + value + ', #question-table').find('.add-comment-link').click();
                                $('#answer-container-' + value + ', #question-table').find('.commentBox').val(text);
                                window.onbeforeunload = null;
                                window.location = '#comment-' + value + '-form';
                            }

                            close();
                        }
                    },
                    'event': e,
                    show_no: true
                });
            }

            if (possibleLocations) {
                showDialog(possibleLocations);
                return;
            }

            possibleLocations = 'fetching';

            $.getJSON('/questions/372795/postLocations.json', showDialog).complete(function() {
                window.setTimeout(function() {
                    if ($('#is-answer-selector').length == 0) {
                        possibleLocations = false;
                    }
                }, 250);
            });
        } catch (e) {
            isAnswerCallback();
        }
    }

    function appendToForm($form, logTime, observations, markAnswered) {
        var $logTime = $form.find('[name=logTime]');

        if ($logTime.length == 0) {
            $form.append('<input type="hidden" name="logTime" value="' + logTime + '" />');
        } else {
            $logTime.val(logTime);
        }

        var $observations = $form.find('[name=observations]');

        if ($observations.length == 0) {
            $form.append('<input type="hidden" name="observations" value="' + observations + '" />');
        } else {
            $observations.val(observations);
        }

        var $markAnswered = $form.find('[name=markAnswered]');

        if (markAnswered && $markAnswered.length == 0) {
            $form.append('<input type="hidden" name="markAnswered" value="true" />');
        } else if (!markAnswered && $markAnswered.length > 0) {
            $markAnswered.remove();
        }
    }

    function answerSubmiissionCallback(logTime, observations, markAnswered) {
        var $form = $('#fmanswer');
        appendToForm($form, logTime, observations, markAnswered);
        window.onbeforeunload = null;
        $form.submit();
    }


    $('.submit').click(function(e) {
        e.preventDefault();

        if (currentUser == null) {
            $('#fmanswer').submit();
            return false;
        }

        confirmIsAnswer(e, function() {
            window.onbeforeunload = null;
            $('#fmanswer').submit();
        });
        return false;
    });

});
</script> 
