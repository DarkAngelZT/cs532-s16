    <script type="text/javascript">
        var cat_array = [];
        var type_array = [];
        
        $("form#footer-search div.search-options input.category").click(function() {
            var search_categories = "";
            var catid = $(this).val();
            if ($.inArray(catid,cat_array) > -1) {
                cat_array.splice($.inArray(catid,cat_array),1);
            }
            else {
                cat_array.push(catid);
            }
            $.each(cat_array, function( index, value ) {
                search_categories += value;
                var last = cat_array[cat_array.length-1];
                if (value != last) {
                    search_categories += ',';
                }
            });
            $("input#sectionCollectionFooter").val(search_categories);
        });
        
        $("form#footer-search div.search-options input.type").click(function() {
            var search_types = "";
            var typeid = $(this).val();
            if ($.inArray(typeid,type_array) > -1) {
                type_array.splice($.inArray(typeid,type_array),1);
            }
            else {
                type_array.push(typeid);
            }
            $.each(type_array, function( index, value ) {
                search_types += value;
                var last = type_array[type_array.length-1];
                if (value != last) {
                    search_types += ',';
                }
            });
            $("input#typeCollectionFooter").val(search_types);
        });
    </script>
    
    
    
    


