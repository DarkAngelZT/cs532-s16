   function close_currency_popup(){
        $('.new_currencyInfo').hide();
        $.ajax({
                url: '/vars/currency.php',
                type: 'post',
                data: { cur_popup: 'true' },
            }).done(function(data) {
            }).fail(function() {
            }).always(function() {
            });
    }

    function handleCoupon(data, valBut) {
        var obj = jQuery.parseJSON(data);
        $( "#errorMessage1" ).remove();
        if(obj.status == 'login')
        {
            $('.redeemLogin').trigger('click');
        }   
        else if(obj.status == 'applied')
        {
            location.reload(true);
            /*
            $('.txtCouponCode' + valBut).addClass('error');
            if(valBut != 2)
                $('.imgCross' + valBut).show();
            $('.tooltip-bottom').hide();
            $('.txtCouponCode' + valBut).val('Already Applied');
            var errorM = '<div id="errorMessage1" class="clearfix _kt_content_wrapper automargin signuperror ktt_globelError"><div class="ktt_globelErrorInner"> <div class="ktt_globelErrorMessage">      <h2 class=""><span class="error">Hey ' + obj.fname +'! You have already used this Impact Code on Ketto. Thank you for your support.</span></h2>    </div>   </div>  </div>';
            $( "body" ).append( errorM);
            setTimeout(function() { $("#errorMessage1").hide('blind', {}, 1000) }, 5000);
            */
        } 
        else if (obj.status == 'contribute')
        {
            $('.overlay-full').show();
            var formInputs = '<input type="hidden" name="form_action" value="pay" />' +
                                '<input type="hidden" name="PaymentMethod" value="coupons" />' +
                                '<input type="hidden" name="fmd_donor_email" value="'+ obj.email + '" />'+ 
                                '<input type="hidden" name="fmd_donor_first_name" value="'+ obj.fname + '" />'+
                                '<input type="hidden" name="fmd_id" value="5567" />'+
                                '<input type="hidden" name="fmd_campaign_id" value="5567" />'+
                                '<input type="hidden" name="fmd_donated_amount" value="'+ obj.amount + '" />'+
                                //'<input type="hidden" name="status_flag" value="5" />'+
                                '<input type="hidden" name="fmd_payment_type" value="indian" />';
            $('<form action="/contribute/contribute_now1.php" method="POST" > '+ formInputs +' </form>').appendTo('body').submit();
        }   
        else
        {
            $('.txtCouponCode' + valBut).addClass('error');
            $('.tooltip-bottom').hide();
            if(valBut != 2)
                $('.imgCross' + valBut).show();
            $('.txtCouponCode' + valBut).val('Invalid Code');
            var errorM = '<div id="errorMessage1" class="clearfix _kt_content_wrapper automargin signuperror ktt_globelError"><div class="ktt_globelErrorInner"> <div class="ktt_globelErrorMessage">      <h2 class=""><span class="error">Impact code does not exist. Please re-check and try again.</span></h2>    </div>   </div>  </div>';
            $( "body" ).append( errorM);
            setTimeout(function() { $("#errorMessage1").hide('blind', {}, 1000) }, 5000);
        }    
    }
    function submitContribute(rewardId,donation_amount) {
                var nm = 'reward_id_amt_' + rewardId;
        var campaign_id = 5567;
        var rewardSelectedAmount = $('#form1 input[name=' + nm + ']').val();
        if(donation_amount == 0) 
        {
            var formInputs = '<input type="hidden" name="fmd_id" value="'+ campaign_id +'" />' +
            '<input type="hidden" name="fmd_donated_amount" value="0" />' +
            '<input type="hidden" name="reward_id" value="0" />';
            
        }  
        else
        {
            var formInputs = '<input type="hidden" name="fmd_id" value="'+ campaign_id +'" />' +
            '<input type="hidden" name="fmd_donated_amount" value="'+ rewardSelectedAmount + '" />' +
            '<input type="hidden" name="reward_id" value="'+ rewardId + '" />';
        }
                    $('<form action="/contribute/contribute.php" method="POST" > '+ formInputs +' </form>').appendTo('body').submit();
                
    }
    
    function loadRewardsPopup(rewardId) {
        submitContribute(rewardId);
        throw new Error("Something went badly wrong!");
    
        $('.btn-donate').trigger('click');
        var rewardSelectedCounter =0;
        $('#form1 input[name=reward_id][value=' + rewardId + ']').prop("checked", true);
        var nm = 'reward_id_amt_' + rewardId;
        var rewardSelectedAmount = $('#form1 input[name=' + nm + ']').val();
        $('#form1 .ContributeAmount').val(rewardSelectedAmount);
        $('#form1 input[name=reward_id]').each(function() {
//            if($(this).val() == '-' || $(this).val() == rewardId){
            if ($(this).val() == rewardId) {
                $(this).parent().parent().show();
                $('#form1 .ContributeAmount').hide();
                $('#donationPopup').hide();
                rewardSelectedCounter++;
            } else {
                $(this).parent().parent().hide();
            }
        })
        if( rewardSelectedCounter == 1)
            $('.selectRewardsTextJS').html('Your Reward');
        else
            $('.selectRewardsTextJS').html('Select your Reward');
    }
    
        function loadBackers(){
        load=false;
        if($("#CauseBackers ul li").length < 22)
        {
            $('#loadingbackers').show();
            $('#loadmorebackers').hide();
            $.ajax({
                url: '/campaign/campaign_details.php',
                type: 'GET',
                data: {
                    p: page_count,
                    fmd_id: '5567'
                }
            }).done(function(data) {
                var dataLength = data.length;
                if (dataLength > 10) {
                    page_count++;
                    $(".donationHistoryListing").append(data);
                    if($("#CauseBackers ul li").length < 22)
                    {
                        $('#loadmorebackers').show();
                    }
                    $('#loadingbackers').hide();
                }
            }).fail(function() {
                markStatus(data);
            }).always(function() {
                load=true;
            });
        }
    }
    
    function checkReward(submit) {
        var filledAmount = $('#form1 .ContributeAmount').val();
        var rewardSelected = $('#form1 input[name=reward_id]:checked').val();
        var errorMessSpan = $('#form1 .errorMessage');

        if (rewardSelected != undefined && rewardSelected != '-') {
            $('#form1 .ContributeAmount').hide();
            $('#donationPopup').hide();
            var nm = 'reward_id_amt_' + rewardSelected;
            var rewardSelectedAmount = $('#form1 input[name=' + nm + ']').val();
            rewardSelectedAmount = parseFloat(rewardSelectedAmount);
            filledAmount = parseFloat(filledAmount);
            if (!$.isNumeric(filledAmount)) {// || filledAmount < rewardSelectedAmount){
                $('#form1 .ContributeAmount').val(rewardSelectedAmount);
                return false;
            }
            if(submit == 0)
            {}
            else
            {    
                $('#form1 .ContributeAmount').val(rewardSelectedAmount);
            }
        }
        
        var clickedButton =$('#form1 input[name=fmd_payment_type]').val();
        //alert(clickedButton);
        if (rewardSelected != undefined && rewardSelected != '-' ) {
            var nm = 'reward_id_amt_usd_' + rewardSelected;
            var rewardSelectedAmountUSD = $('#form1 input[name=' + nm + ']').val();
            rewardSelectedAmountUSD = parseFloat(rewardSelectedAmountUSD);
            filledAmount = parseFloat(filledAmount);
            if (rewardSelectedAmountUSD > filledAmount && clickedButton == 'Foreign Donors (USD)') {
                errorMessSpan.addClass('error');
                errorMessSpan.html('To avail reward offer<br>Minimum amount - $' + rewardSelectedAmountUSD);
                errorMessSpan.show();
                return false;
            } else if (clickedButton == 'Foreign Donors (USD)'){    
                var amount_usd = 60 * rewardSelectedAmountUSD;
                $('#form1 .ContributeAmount').hide();
                $('#form1 .ContributeAmount').val(amount_usd);
                $('#contributetext').show();
                
            }    
            var nm = 'reward_id_amt_' + rewardSelected;
            var rewardSelectedAmount = $('#form1 input[name=' + nm + ']').val();
            rewardSelectedAmount = parseFloat(rewardSelectedAmount);
            filledAmount = parseFloat(filledAmount);
            //if (rewardSelectedAmount > filledAmount){//  && clickedButton == 'Indian Donors (INR)') {
            if (rewardSelectedAmount > filledAmount  && clickedButton == 'Contribute Now') {
                errorMessSpan.addClass('error');
                //errorMessSpan.text('Entered amount is less than selected reward.')
                //errorMessSpan.text('Donation sum is less than chosen reward sum');
                errorMessSpan.html('To avail reward offer<br>Minimum amount - Rs. ' + rewardSelectedAmount);
                errorMessSpan.show();
                //errorMessSpan.fadeOut(5000, function() {
                //});
                return false;
            }
        }
        else
        {
            $('#donationPopup').show();
            $('#form1 .ContributeAmount').show();
            if(submit != 0)
            { 
                $('#form1 .ContributeAmount').val('');
                $('#form1 .ContributeAmount').focus();
            }
        }    

        var filledAmount = $('#form1 .ContributeAmount').val();
        if (!$.isNumeric(filledAmount) && submit == 0) {
            errorMessSpan.addClass('error');
            errorMessSpan.text('No Special Characters allowed.');
            errorMessSpan.show();
            return false;
        }
        if (filledAmount < 10 && (rewardSelected == undefined || rewardSelected == '-') && submit == 0) {
            errorMessSpan.addClass('error');
            errorMessSpan.text('Minimum Donation is Rs 10. Please Change your amount.');
            errorMessSpan.show();
            return false;
        }
        

        errorMessSpan.hide();
        return true;
    }

    function checkNSubmit() {
        if (!checkReward(0))
            return false;

        var errorMessSpan = $('#form1 .errorMessage');
        var payment_type = $('#form1 input[name=fmd_payment_type]').val()
        payment_type = $.trim(payment_type);
        if (payment_type.length == 0) {
            errorMessSpan.addClass('error');
            errorMessSpan.text('Please click the donor button')
            errorMessSpan.show();
            return false;
        }

        if (payment_type == 'Foreign Donors (USD)') {
            $('#form1 input[name=fmd_payment_type]').val('foreign')
        } else {
            $('#form1 input[name=fmd_payment_type]').val('indian')
        }
        errorMessSpan.hide();
        return true;
    }

    function showCopyDialog(ele) {
        $("#" + ele).dialog({
            height: 250,
            width: 440,
            modal: true,
            buttons: {
                Ok: function() {
                    $(this).dialog("close");
                }
            }
        });
    }

    function detectScroll() {
        var curr_nav_ht = $("#CampaignDetailsSidebar #Rewards").height();
        var listings_ht = $("#CampaignDetailsSidebar .RewardsListing").height();
        if ((window.pageYOffset + 50 + 72) >= wrapper_top) {    // 50 = fixed header height, 72 = height of sticky contribute bar
            // console.log(listings_ht + ">" + (window.innerHeight - 70));
            if (listings_ht >= (window.innerHeight - 50 - 72 - 37)) {
                $("#CampaignDetailsSidebar .RewardsListing").css({
                    "height": window.innerHeight - 50 - 72 - 20 + "px",
                    "overflow-y": "scroll"
                });
            }
            else {
                $("#CampaignDetailsSidebar .RewardsListing").css({
                    "height": "auto",
                    "overflow-y": "hidden"
                });
            }
            if (((window.pageYOffset + 50 + 72 + 10) + curr_nav_ht) > footer_top) {
                $("#CampaignDetailsSidebar #Rewards").removeClass("fixPos");
                $("#CampaignsDetails").addClass("fixpos");
                $("#CampaignDetailsSidebar #Rewards").addClass("fixPosbott");
            } else {
                $("#CampaignsDetails").removeClass("fixpos");
                $("#CampaignDetailsSidebar #Rewards").removeClass("fixPosbott");
                $("#CampaignDetailsSidebar #Rewards").addClass("fixPos");
            }
        } else {
            $("#CampaignsDetails").removeClass("fixpos");
            $("#CampaignDetailsSidebar #Rewards").removeClass("fixPos");
            $("#CampaignDetailsSidebar #Rewards").removeClass("fixPosbott");
            $("#CampaignDetailsSidebar .RewardsListing").css({
                "height": "auto",
                "overflow-y": "hidden"
            });
        }
    }

wizrocket.event.push("Campaign Viewed", {"Campaign Name":"Implementing Game Based Learning in Schools for Effective Learning", "Campaign Id":"5567" });
</script>

