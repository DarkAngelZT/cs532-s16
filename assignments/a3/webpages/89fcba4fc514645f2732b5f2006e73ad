<script language="javascript">
var stop;
var playerisready=0;
var XMLHttpRequestObject = false;

if (window.XMLHttpRequest) {
        XMLHttpRequestObject = new XMLHttpRequest();
} else if (window.ActiveXObject) {
        XMLHttpRequestObject = new ActiveXObject("Microsoft.XMLHttp");
}

function getData(dataSource) {
        if(XMLHttpRequestObject) {
                XMLHttpRequestObject.open("GET", dataSource);
                XMLHttpRequestObject.send(null);
        }
}

function loadvideo(id,titolo,titoletto) {
	dmplayer=document.getElementById('mydmplayer');
	dmplayer.loadVideoById(id);
	t1=document.getElementById('vidtitolo');
	t2=document.getElementById('vidtitoletto');
	t1.innerHTML=titolo;
	t2.innerHTML=titoletto;
}

function repospopupvideo () {
        e=document.getElementById('popupvideolyr');
        e.style.left=((document.documentElement.clientWidth/2)-450)+'px';
}

function closepopupvideo () {
	dmplayer=document.getElementById('mydmplayer');
	//dmplayer.seekTo(0);
	dmplayer.stopVideo();
	e2=document.getElementById('popupvideolyr');
	b=document.getElementById('obdy');
	e2.style.visibility='hidden';
	b.style.filter='alpha(opacity=100)';
	b.style.opacity='1';
	playerisready=0;
}

function showpopupvideo(id) {
        b=document.getElementById('obdy');
        b.style.filter='alpha(opacity=50)';
        b.style.opacity='0.5';
        repospopupvideo();
        e2=document.getElementById('popupvideolyr');
        e2.style.visibility='visible';
	dmplayer=document.getElementById('mydmplayer');
	if(dmplayer) {
		dmplayer.loadVideoById(id);
	} else {
		loadplayer();
        	setTimeout("startplayer('"+id+"')",1000);
	}
}

function startplayer(id) {
        if(playerisready) {
		dmplayer=document.getElementById('mydmplayer');
		dmplayer.loadVideoById(id);
        } else {
        	setTimeout("startplayer('"+id+"')",1000);
	}
}

function onDailymotionPlayerReady(playerid) {
	if(playerid=="dmplayer") {
        	playerisready=1;
	}
}

function updBox(w) {
	titolo=document.getElementById('titolo_box');
	im=document.getElementById('img_box');
	testo=document.getElementById('testo_box');
/*

                     <tr><td id="img_box"><a href="index.php?page=interview&cod="><img border="0" align="top" src="showimg.php?cod=&tp=int&resize=7"></a></td><td valign="top" id="testo_box"><div class="small_blue_bold"></div><div class="small_grey"><b>by </b></div><br><a href="index.php?page=interview&cod=">Read more</a></td></tr>
*/

	switch(w) {
		case 'edi':
		titolo.innerHTML="Editorial";
		im.innerHTML='<a href="index.php?page=editorial&cod=26"><img border="0" align="top" src="images/gianni.jpg" height="100"></a>';
		testo.innerHTML='<div class="small_blue_bold">TRANSPARENCY is the foremost priority - AIPS Magazine II/11</div><div class="small_grey"><b>by Gianni Merlo</b></div><br><a href="index.php?page=editorial&cod=26">Read more</a>';
		break;
		case 'foc':
		titolo.innerHTML="Focus";
		im.innerHTML='<a href="index.php?page=focus&cod=89"><img border="0" align="top" src="showimg.php?cod=89&tp=f&resize=7"></a>';
		testo.innerHTML='<div class="small_blue_bold">The Ethics \'Iron Lady\' of the Olympics</div><div class="small_grey"></div><br><a href="index.php?page=focus&cod=89">Read more</a>';
		break;
		case 'int':
		titolo.innerHTML="Interview";
		im.innerHTML='<a href="index.php?page=interview&cod=17"><img border="0" align="top" src="showimg.php?cod=17&tp=int&resize=7"></a>';
		testo.innerHTML='<div class="small_blue_bold">AIPS Treasurer Jorge Ribeiro in conversation with Korean referee IM Eun-Ju </div><div class="small_grey"><b>by Jorge Ribeiro, AIPS Treasurer</b></div><br><a href="index.php?page=interview&cod=17">Read more</a>';
		break;
		case 'prf':
		titolo.innerHTML="Profiles";
		im.innerHTML='<a href="index.php?page=profile&cod=5"><img border="0" align="top" src="showimg.php?cod=5&tp=prf&resize=7"></a>';
		testo.innerHTML='<div class="small_blue_bold">Hedrick Chad - Speed Skating</div><div class="small_grey"></div><br><a href="index.php?page=profile&cod=5">Read more</a>';
		break;
		case 'tio':
		titolo.innerHTML="Timeout";
		im.innerHTML='<a href="index.php?page=timeout&cod=18"><img border="0" align="top" src="showimg.php?cod=18&tp=tio&resize=7"></a>';
		testo.innerHTML='<div class="small_blue_bold">One Oath is missing!</div><div class="small_grey"><b>by Christer Ahl</b></div><br><a href="index.php?page=timeout&cod=18">Read more</a>';
		break;
	}
}

var l=0;
var r=0;

function doScrollVideo(dir)
{
	o=document.getElementById('videoscr');
	switch(dir) {
		case 'left':
		o.scrollLeft -= 107; //o.clientWidth
		break;
		case 'right':
		o.scrollLeft += 107;
		break;
	}
	if(stop)
		return;

	//setTimeout('doScrollVideo(\''+dir+'\')',10);
}

/*
function startScrollVideo(dir) {
	stop=0;
	setTimeout('doScrollVideo(\''+dir+'\')',10);
}

function stopScrollVideo() {
	stop=1;
}
*/

function doScroll(dir)
{
	o=document.getElementById('galleryscr');
	switch(dir) {
		case 'left':
		o.scrollLeft -= 2; //o.clientWidth
		break;
		case 'right':
		o.scrollLeft += 2;
		break;
	}
	if(stop)
		return;

	setTimeout('doScroll(\''+dir+'\')',3);
}

function startScroll(dir) {
	stop=0;
	setTimeout('doScroll(\''+dir+'\')',3);
}

function stopScroll() {
	stop=1;
}

window.onresize=repospopupvideo;
