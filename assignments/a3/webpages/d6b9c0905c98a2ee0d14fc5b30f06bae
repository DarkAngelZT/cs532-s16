<div class="scroll-wrapper"> <div data-component="scrollbarArea" data-scrollbar-area-options='{"createWrapper":false,"disableBodyScroll":"true","scrollx":"false","extraScrollElement":"#galleryPage","verticalKeyboardScroll":true,"enableTracking":false}'> 
