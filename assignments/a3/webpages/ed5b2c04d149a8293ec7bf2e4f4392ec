var authorFinal = '';
if ( authorRaw.length > 0 ) {
var regRemovables = /\bby\b\s?(?:\-|:)?\s?|\w+@\w+\.\w{2,3}|@\w+|\./gmi;
var regReplacables = /\s*(?:,?\s?\band\b|\+|\/|&|,|;|\|)\s*/gmi;
var $author = $(authorRaw);
var $tempAuthor = $('<div/>').append($author);
authorText = $tempAuthor.find('.ng_byline_name').text() || $author.text() || authorRaw;
authorFinal = authorText.replace(regRemovables, '').replace(regReplacables, ',');
}
var _sf_async_config = _sf_async_config || {};
/** CONFIGURATION START **/
_sf_async_config.sections = mi.pageInfo.getConf('marketInfo.domain');
_sf_async_config.authors = authorFinal;
