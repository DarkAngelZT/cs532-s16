$('.show-reviewer').live('click',function()
{
	var reviewHtml = "";
	var url = $(this).attr("data-href");
	var title = $(this).attr("data-title");
	reviewHtml += "<h3 style='font-size:1.5rem;font-weight:bold;margin-top: 0px;margin-bottom: 0px;'><span class='fa fa-comments-o'></span>"+title+"レビュー一覧</h3>";
	reviewHtml += "<iframe  frameborder='0' src='"+ url +"' style='height:100%;width:100%'></iframe>";
	reviewHtml += "</div>";

	bootbox.dialog({
	      backdrop  : true,
      keyboard  : true,
      show      : true,
	  message: reviewHtml,

	});
	return false;

});



$('th.current-price').click(function()
{
	var order = 3;
	if($("#MediaVIRSORT").val() == "3")
	{
		order = 4;
	}
	$("#MediaVIRSORT").val(order);
	$('#sort-form').submit();
	return false;
});

$('th.sales-rank').click(function()
{
	var order = 1;
	if($("#MediaVIRSORT").val() == "1")
	{
		order = 2;
	}
	$("#MediaVIRSORT").val(order);
	$('#sort-form').submit();
	return false;
});



$('.filter-commit').click(function()
{
	$('#sort-form').submit();
	return false;
});

$('.filter-cancel').click(function()
{
	$("#MediaVIRSORT").val("0");
	$.cookie("COOKIE_SHOW_OPTION","0", { expires: 30 });
	$('#sort-form').submit();
	return false;
});

$('.filter-all-select').click(function(){

	$('.sort-container input[type="checkbox"]').attr("checked","checked");
	return false;
});
$('.filter-all-unselect').click(function(){

	$('.sort-container input[type="checkbox"]').removeAttr("checked");
	return false;
});

$('#sort-container-toggle').click(function()
{
	if($(".sort-container").is(":hidden"))
	{
		$('.sort-container').show();
		$.cookie("COOKIE_SHOW_OPTION","1", { expires: 30 });

	}
	else
	{
		$('.sort-container').hide();
		$.cookie("COOKIE_SHOW_OPTION","0", { expires: 30 });
	}
	return false;
})

$('#display-toggle').click(function()
{
	toggleDisplay();
	return false;
})
function toggleFirstDisplay()
{
		var showDisplay = $.cookie("COOKIE_SHOW_DISPLAY");
		if(showDisplay == 1)
		{
			$("#display-thumbnail").show();
			toggleDisplay();
		}
		else
		{
			$("#display-thumbnail").show();
		}

		var showOption = $.cookie("COOKIE_SHOW_OPTION");
		if(showOption == 1)
		{
			$('.sort-container').show();
		}
}
function toggleDisplay()
{
	if($("#display-thumbnail").is(":hidden"))
	{
		$('#display-table').hide(0,function()
		{
			$('#display-thumbnail').show();
		});
		$('#display-toggle').html("<span class='fa fa-list fixed'></span>");

		$.cookie("COOKIE_SHOW_DISPLAY","0", { expires: 30 });
	}
	else
	{
		$('#display-thumbnail').hide(0,function(){

			$('#display-table').show();
		});
		$('#display-toggle').html("<span class='fa fa-th-large fixed'></span>");
		$.cookie("COOKIE_SHOW_DISPLAY","1", { expires: 30 });
	}
}
toggleFirstDisplay();


	$('p.lazy').lazyload({
	        effect: 'fadeIn',
	        effectspeed: 200
	});






