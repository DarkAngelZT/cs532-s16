<!-- mobile redirect -->
<script type="text/javascript">
    //there is already a GetQueryStringValue and trim function in overal-utils, but this
    //is necessary so that we can use the function in TN_mobileUrlOverride before loading the main scripts
    function GetQueryStringValueMobile(key, queryString) {
        if (queryString === undefined)
            queryString = window.location.search;
        if (queryString.length > 0) {
            var objURL = new Object();
            queryString.replace(
                new RegExp("([^?=&]+)(=([^&]*))?", "g"),
                function ($0, $1, $2, $3) {
                    objURL[$1] = $3;
                }
            );
            if (objURL[key]) {
                return objURL[key];
            }
        }
        return false;
    }

    String.prototype.lrtrim = function () {
        return this.replace(/^\s+|\s+$/g, "");
    }

    function TN_mobileUrlOverride() {
        TN_mobile.OtherMobileURL = TN_mobile.DroidUrl = TN_mobile.IphoneUrl = "http://www.ticketnetwork.com/tn_mobile.aspx";
        var source = GetQueryStringValueMobile('utm_source');
        var medium = GetQueryStringValueMobile('utm_medium');
        if(source && medium) {
            source = source.lrtrim().toLowerCase();
            medium = medium.lrtrim().toLowerCase();
            if(source == 'google' && (medium.indexOf('ppc') != -1 || medium.indexOf('cpc') != -1))
            TN_mobile.OtherMobileURL = TN_mobile.DroidUrl = TN_mobile.IphoneUrl = "http://www.ticketnetwork.com/tn_mobilePPC.aspx";
        }
    }
</script>




