    $.ajax({
      type: 'POST',
      url: '/gp/redirection/india.html',
      data: {
        path: '/Fire-of-the-Gods-Fitness/dp/B0035WUBRM',
        queryString: '?ie=UTF8\u0026%2AVersion%2A=1\u0026%2Aentries%2A=0',
        pageType: 'Detail',
        referer: ''
      },
      success: function(data) {
        if (data) {
          $('body').append(data);
        }
      }
    });
  });
</script>

    








  




