<script>
    $(function() {

        //input.search-but
        $('body').on('click', 'input.search-but', function(e){
            e.preventDefault;
                if ($(this).parents('form').find('input[value=video]').is(':checked'))
                {
                    var form =$(this).parents('form');
                    var search_string =form.find('input[name=searchm]').val();
                    document.location.href='/video?q='+search_string;
                } else {
                    $(this).parents('form').submit();
                }
            return false;
        });
        //, input[name=search_button]
        $('body').on('click', 'input[name=search_button]', function(e){
            e.preventDefault;
            if ($(this).parents('form').find('input[value=video]').is(':checked'))
            {
                var form =$(this).parents('form');
                var search_string =form.find('input[name=search]').val();
                document.location.href='/video?q='+search_string;
            } else {
                $(this).parents('form').submit();
            }
            return false;
        });

        $('#date_from').dateRangePicker({
            autoClose: true,
            format: 'DD.MM.YYYY',
            singleDate : true,
            showShortcuts: false 
        });

        $('#date_to').dateRangePicker({
            autoClose: true,
            format: 'DD.MM.YYYY',
            singleDate : true,
            showShortcuts: false 
        });
    });
</script>

