    function DropDown(el) {
        this.dd = el;
        this.initEvents();
    }
    DropDown.prototype = {
        initEvents: function () {
            var obj = this;

            obj.dd.on('click', function (event) {
                $(this).toggleClass('active');
                event.stopPropagation();
            });
        }
    }

    $(function () {

        var dd = new DropDown($('#dd'));

        $(document).click(function () {
            // all dropdowns
            $('.wrapper-dropdown-2').removeClass('active');
        });

    });

    function chgText(n1, v1) {
        $('#txType').html(n1);
        document.getElementById("ctrl_topSearch1_hdnMainType").value = v1;
    }

    if (document.getElementById("ctrl_topSearch1_hdnMainType").value == "-3") {
        $('#txType').html('AGRICULTURAL');
    } else if (document.getElementById("ctrl_topSearch1_hdnMainType").value == "-2") { $('#txType').html('COMMERCIAL'); } else { $('#txType').html('RESIDENTIAL'); }
