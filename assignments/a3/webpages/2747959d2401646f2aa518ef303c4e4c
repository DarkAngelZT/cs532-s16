<script src="http://cd-n.co.uk/assets/magnific-popup.js"></script>

<script>
	function openPopUP() {
  	window.open('mywindow.html','NewWin', 'toolbar=no,status=no,width=350,height=135')
	}	

	$('.popup').click(function (event) {
    event.preventDefault();
    window.open($(this).attr("href"), "popupWindow", "width=100,height=100,scrollbars=yes");
	});	

		
		

		
	$('.open-popup-link').magnificPopup({
  type:'inline',
  midClick: true,
		// Allow opening popup on middle mouse click. Always set it to true if you don't provide alternative source in href.
});	
		
	$( ".gamecover" ).hover(function() {
			$( this ).find('.fromprice').css('background','#000');
		}, function() {
			$( this ).find('.fromprice').css('background','#fb8a2e');
		}
	);	

		
	$( ".menutoggle" ).click(function() {
		$('.mobile-navigation').slideToggle('fast');
	});	

	$( ".wpcf7-submit" ).click(function() {
		$('html, body').animate({
			scrollTop: $('.wpcf7-form').offset().top-150
		}, 1000);
	});	

	$( window ).resize(function() {
		if ($(window).width() > 650) {
		   $('.mobile-navigation').hide();
		} 
	});

	$(window).scroll(function () {
		if ($(this).scrollTop() > 100) {
			$('.scrollup').fadeIn();
		} else {
			$('.scrollup').fadeOut();
		}
	});

	$('.scrollup').click(function () {
		$("html, body").animate({ scrollTop: 0 }, 600);
		return false;
	});	
	</script>

