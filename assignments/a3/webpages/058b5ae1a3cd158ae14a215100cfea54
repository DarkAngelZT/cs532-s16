<script type="text/javascript">
$(function() {

//PickupList
  $("#modPickupList .linkOther li").each(function() {
    $("#modPickupList li:last-child").css('margin-bottom', '0');
  });

  $('p.btnAppear').click(function() {
    if ($(".selectArea").hasClass('selectAreaAppear')) {
      $(".selectArea").removeClass('selectAreaAppear');
      $(this).attr('id', 'selectOnClick');
    }
    else {
      $('.selectAreaAppear').removeClass('selectAreaAppear');
      $(".selectArea").addClass('selectAreaAppear');
      $(this).attr('id', 'selectOffClick');
    }
  });

 $('li.selectPulldown').click(function() {
    $(this).children('ul.selectChild').toggle();
 });


//searchArea-hover
  $('.searchArea button').hover(
    function(){
      $(this).addClass('hover');
    },
    function(){
      $(this).removeClass('hover');
    }
  );

//list border-bottom
  $("#modList .eachBox .glanceArticleBox li").each(function() {
    $("#modList .eachBox .glanceArticleBox li:last-child").css('border-bottom', 'none');
  });

//.glanceArticleBox article-hover
  $('#modResultList .glanceArticleBox article').hover(
    function(){
      $(this).addClass('hover');
    },
    function(){
      $(this).removeClass('hover');
    }
  );

//list hover
  $('#modRankingList li').hover(
    function() {
      $(this).addClass('hover');
    },
    function() {
      $(this).removeClass('hover');
    }
  );
});

//list hover
  $('.sectionList li').hover(
    function() {
      $(this).addClass('hover');
    },
    function() {
      $(this).removeClass('hover');
    }
  );

