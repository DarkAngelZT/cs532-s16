                    <script type="text/javascript">
                        function renderUpdates(count, page){
                            for(var i = page*count; i < count+(page*count); i++){
                                $("#UpdatesListing").append(updates_template(updates_arr[i]));                                
                            }       
                        }
                        function loadUpdates(){                            
                            update_page++;
                            var rendered_updates = $("#UpdatesListing .ngo_update_content_wrap").length;
                            rendered_updates = rendered_updates+2;
                            if(rendered_updates>=updates_arr.length){
                                $(".ngo_updates_mainwrapper .view-updates-content").text("No more updates!")
                            }                            
                            renderUpdates(disp_update_count,update_page);
                            return false;
                        }
                        var updates_source = $("#updates-template").html();
                        var updates_template = Handlebars.compile(updates_source);
                        var update_html = false;
                        Handlebars.registerHelper('formatDate', function(context) {
                            var context_date = new Date(context);
                            var Months_Arr = ['January', 'February', 'March', 'April', 'May', 'June', 'July', 'August', 'September', 'October', 'November', 'December'];
                            return Months_Arr[context_date.getMonth()]+" "+context_date.getDate()+", "+context_date.getFullYear();
                        });
                        var disp_update_count = 2;
                        var update_page = 0;
                        renderUpdates(disp_update_count,update_page);
                        
                    </script>
