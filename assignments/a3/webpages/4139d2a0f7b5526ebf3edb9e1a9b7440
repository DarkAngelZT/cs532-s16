        ibsys.userProfile.userProfileInit.serviceUrl = '/servlet/profile/user-scope/sites/bend/user-profiles';
        ibsys.userProfile.userProfileInit.sessionUrl = '/servlet/profile/user-scope/sites/bend/sessions';
        ibsys.userProfile.userProfileInit.sitekey = 'bend';
        ibsys.userProfile.userProfileInit.siteName = 'KTVZ';
        ibsys.userProfile.userProfileInit.offersEndpoint = '/servlet/profile/sites/npgb-bend/offers';
        ibsys.userProfile.userProfileInit.subscriptionsEndpoint = '/servlet/profile/user-scope/sites/bend';
        ibsys.userProfile.userProfileInit.ibOauthUrl = 'https://id.ib-prod.com/oauth-provider';
        console.debug('init userProfile');
        ibsys.vm.userProfile = new ibsys.userProfile.UserProfile();
    </script>
