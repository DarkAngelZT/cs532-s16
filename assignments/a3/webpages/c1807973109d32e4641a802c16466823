<script type="text/javascript">
$(document).ready(function() {
	// $(".header_google_search .gsc-input").attr("placeholder","Search");
	$(".various").fancybox({
		'titlePosition': 'inside',
		'transitionIn': 'elastic',
		'transitionOut': 'elastic',
		'overlayColor': '#000',
		'overlayOpacity': 0.7,
		'padding': 0,
		'easingIn':'easeOutBack',
		'easingOut':'easeInBack',
		'autoDimensions': true,
		'centerOnScroll': true,						
		helpers: {
			overlay : { 
				closeClick: false 
			} 
		},
		keys: {
			close : null
		}
	});

	$(window).on("scroll", makeHeaderTransparent);
	$("body").on("click", function() {
		if ($("#SmallScreenHeaderActions > li ul").is(":visible"))
			$("#SmallScreenHeaderActions > li ul").hide();						
	});
	$("#SmallScreenHeaderActions > li > a").on("click", function(event) {
		event.preventDefault();
		event.stopPropagation();
		$(this).parent().find("ul").toggle();
	});
	$("#UserImage").click(function(){
    	$("#userActions").toggle();                                                   
	});
});
function makeHeaderTransparent() {
	var window_scrollx = parseInt($(window).scrollTop());
	if (window_scrollx > 5) {
		$("._kt_header_wrapper").addClass('opaque');
	} else {
		$("._kt_header_wrapper").removeClass('opaque');
	}	
}
</script>
