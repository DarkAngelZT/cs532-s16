				<div class="test">
					
											
						<script type="text/javascript">
							$(function() {
								
								$(".test_load").fadeOut(500, function() {
						
									$(".test_opening").fadeIn(500);
									
								});
								
								
								
								point_result={};
								answer_clicked="";
								for(i=1;i<=7;i++){
									point_result[i]={};
								}
								$(document).on('click','.test_opening_start', function(){
									
									$(".test_question:eq("+0+")").children(".test_question_imgtext").children(".test_question_imgtext_img").css({"background-image":"url('"+ $(".test_question:eq("+0+")").children(".test_question_imgtext").children(".test_question_imgtext_img").attr("data-original") +"')"});
										
									$(".test_question:eq("+0+")").children(".test_question_answer").each(function(){
										$(this).children(".test_question_answer_img").css({"background-image":"url('"+ $(this).children(".test_question_answer_img").attr("data-original") +"')"});
									});
									
									$(".test_opening").fadeOut(500, function() {
										$(".test_question_header").fadeIn(500);
										$(".test_question_header_number:eq("+0+")").css({"background":"#3498db","color":"#fff"});
										$(".test_question:eq("+0+")").fadeIn(500);
										
									});
							
									$.ajax({
									    url: '/ajax_game_countstart.php',
									    type: 'POST',
									    data: {
									        id_game: 55029									        ,flag_sp: 0
									        ,count_question: 7									        ,count_result: 7									        
										},
									    dataType: 'html'
									})
									.done(function( data ) {
										
									})
									.fail(function( data ) {
									})
									.always(function( data ) {
										
									});
									
								});
								
								$(document).on('click','.test_question_answer', function(){
									
									i=$(this).parent().attr("id");
									
									answer_clicked+=String($(this).parent().children(".test_question_answer").index(this) + 1);
									
									for(j=1;j<=7;j++){
										point_result[i][j]=$(this).attr("point_result_"+j);
									}
										
									if(i!=7){
										
										$(this).parent().next().children(".test_question_imgtext").children(".test_question_imgtext_img").css({"background-image":"url('"+ $(this).parent().next().children(".test_question_imgtext").children(".test_question_imgtext_img").attr("data-original") +"')"});
										
										$(this).parent().next().children(".test_question_answer").each(function(){
											$(this).children(".test_question_answer_img").css({"background-image":"url('"+ $(this).children(".test_question_answer_img").attr("data-original") +"')"});
										});
										
										$(this).parent().fadeOut(500, function() {
										
											$(this).next().fadeIn(500);
											$(".test_question_header_number:eq("+(i) +")").css({"background":"#3498db","color":"#fff"});
											
										
										});
										
									}else{
										
										$(".test_question_header").fadeOut(500);
										
										$(this).parent().fadeOut(500, function() {
										
											point_result_total={};
											
											for(i=1;i<=7;i++){
												for(j=1;j<=7;j++){
													
													if(i==1){
														point_result_total[j]=0;
													}
													
													point_result_total[j] += Number(point_result[i][j]);
													
												}
											}
											
											point_max=0;
											
											for(j=1;j<=7;j++){
												
												if(point_max < point_result_total[j]){
													
													point_max=point_result_total[j];
													
												}
												
											}
											
											point_max_key={};
											k=1;
											
											for(j=1;j<=7;j++){
												
												if(point_max == point_result_total[j]){
													
													point_max_key[k]=j;
													k++;
													
												}
												
											}
											
											if(k==2){
												result_fix=point_max_key[1];
											}else{
												
												
												md5str = CybozuLabs.MD5.calc(answer_clicked);
												
												md5str_num=parseInt(md5str.substring(0, 5),16); 
												
												result_fix=md5str_num%(k-1) + 1;
												
											}
											
											$(".test_result:eq("+(result_fix-1) +")").children(".test_result_img").css({"background-image":"url('"+ $(".test_result:eq("+(result_fix-1) +")").children(".test_result_img").attr("data-original") +"')"});
											
											$(".test_load").fadeIn(250, function() {
												
												$(this).css({"display":"block"});
												
												setTimeout(function(){
													
													$(".test_load").fadeOut(250, function() {
													
														$(".test_result:eq("+(result_fix-1) +")").fadeIn(500);
													
													});
												},250);
												
											});
										
										});
										
									}
									
								});
						
						
								$(document).on('click','.test_result_share_twitter', function(){
									
									$.ajax({
									    url: '/ajax_game_counttweet.php',
									    type: 'POST',
									    data: {
									        id_game: 55029									        ,flag_sp: 0
									        ,count_question: 7									        ,count_result: 7										},
									    dataType: 'html'
									})
									.done(function( data ) {
										
									})
									.fail(function( data ) {
									})
									.always(function( data ) {
										
									});
									
								});
								
								
							});
						</script>
						
						
