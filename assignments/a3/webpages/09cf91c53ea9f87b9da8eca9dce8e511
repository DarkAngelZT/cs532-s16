<script type="text/javascript">
if (window.P && window.P.AUI_BUILD_DATE && typeof P === "object" && typeof P.when === "function") {
	P.when('A', 'ready').execute(function(A) {
        var $ = A.$;

        // Function to have Smooth scroll into view transition
        $.fn.scrollIntoView = function () {
		    return this.each(function () {
		        $('html, body').animate({
		            scrollTop: $(this).offset().top
		        }, 1000);
		    });
		}

		// Enable this for pages that have JS
        $(".a-js .scroll-fragment").bind("click", function(e) {
            e.preventDefault();
            var fragment = $(this).attr("href");
            $(fragment).scrollIntoView();
        });
    });
}
</script>
		



