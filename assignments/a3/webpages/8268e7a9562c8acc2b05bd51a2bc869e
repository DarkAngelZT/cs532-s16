<script type="text/javascript">
	function mobileClick(parent, child, event){
		$(parent).css('background','url(/images/textures/stressed_linen.png)');
		$(child).css('display','block');
		$(child).css('position','absolute');
		$(child).css('z-index','800');
		bindLeftClick();
						//this fixed firefox's middle click issue
						function bindLeftClick(){
							$(document).one('mousedown', function(event){
								if (event.which == 1){
									var target = $(event.target);
									if (!target.parents(parent).length){
										$(child).css('display','none');
										$(parent).css('background','');
									}
								}else{
									bindLeftClick();
								}
							});
						}
					};

										function Ticker(selectors){
						var tickInterval, tickTimer, tickPaused;
						var that = selectors,
						tickNext = function(){
							$(that.ticker + ' li:first').slideUp( function () { $(this).appendTo($(that.ticker)).show(); });
						},
						tickPrev = function(){
							$(that.ticker + ' li:last').hide().prependTo($('#ticker22')).slideDown();
						},
						clickTickNext = function(){
							tickNext();
							setTickTimer(false);
						},
						clickTickPrev = function(){
							tickPrev();
							setTickTimer(false);
						},
						tickPause = function(){
							tickPaused = true;
							clearInterval(tickInterval);
							clearTimeout(tickTimer);
						},
						setTickInterval = function(){
							tickPaused = false;
							tickInterval = setInterval(function(){ tickNext(); }, 10000);
						},
						setTickTimer = function(setIfPaused){
							if(setIfPaused || !tickPaused){
								tickPause();
								tickTimer = setTimeout(function(){
									setTickInterval();
								}, 3000);
							}
						},
						togglePause = function(){
							tickPause();
							$(that.tickPause).hide();
							$(that.tickPlay).show();
						},
						togglePlay = function(){
							setTickInterval();
							$(that.tickPlay).hide();
							$(that.tickPause).show();
						},
						setup = function(){
							setTickInterval();
							$(that.tickNext).on('click', function(){clickTickNext();});
							$(that.tickPrev).on('click', function(){clickTickPrev();});
							$(that.tickPause).on('click', function(){togglePause();})
							$(that.tickPlay).on('click', function(){togglePlay();})
						}
						that.setup = setup;
						return that;
					}
					var podcastEmbedHtml = false;
					var hidePodcastEmbed = function(){
						$('.podcast-live-embed-wrapper').slideUp(function(){
							if(!podcastEmbedHtml)
								podcastEmbedHtml = $(this).parent().html();
							$(this).remove();
						});
					};
					var showPodcastEmbedViaAjax = function(){
						$.ajax({
							url: '/podcast/embed',
							type: 'GET',
							dataType: 'html'
						}).done(function(data){
							podcastEmbedHtml = data;
							showPodcastEmbedViaVar();
						});
					};
					var showPodcastEmbedViaVar = function(){
						$('.podcast-link-live-btn').after(podcastEmbedHtml);
						$('.podcast-live-embed-wrapper').hide().slideDown();
					};
					var showPodcastEmbed = function(){
						if($('.podcast-live-embed-wrapper').length > 0)
							return false;
						if(!podcastEmbedHtml){
							showPodcastEmbedViaAjax();
						}else{
							showPodcastEmbedViaVar();
						}
					};
					var setHiddenPodcastCookie = function(){
						var expireDate = new Date();
						expireDate.setTime(expireDate.getTime()+(24*60*60*1000)); // set to expire in 24 hours
						document.cookie = "hide_live_podcast=1;path=/;expires="+expireDate.toGMTString()+";";
					};
					var deleteCookie = function(name){
						document.cookie = name + '=;path=/;expires=Thu, 01 Jan 1970 00:00:01 GMT;';
					};

										$("[data-ad]").click(function(){
						window.location.href = "/login/index.php";
					});


					$(document).ready(function(){
						$('.flashError').each(function(){
							toastr.error($(this).text());
						});
						var ticker = new Ticker({ ticker:'#ticker22', tickPrev:'.tickPrev', tickNext: '.tickNext', tickPlay: '.tickPlay', tickPause: '.tickPause' });
						ticker.setup();
						$('#header a[href^="/member/#"]').click(function(){
							window.open($(this).attr('href'),'_self');
							window.location.reload();
						});
						$(window).scroll(function(){
							if ($(this).scrollTop() > 100) {
								$('.toTop').fadeIn();
							} else {
								$('.toTop').fadeOut();
							}
						});
						$('.toTop').click(function(){
							$("html, body").animate({ scrollTop: 0 }, 600);
							return false;
						});
						$('.unread').click(function(){
							var img_bm_id = $(this).attr('bm_id');
							$.ajax({
								type: 'POST',
								url: '/global/checkNotifications.php',
								data: {bm_id:img_bm_id},
								success: function(data){
									$('head').append(data);
								}
							});
						});
						$('.acceptAllFriends').click(function(){
							$.ajax({
								type: 'POST',
								url: '/global/checkNotifications.php',
								data: {clearAllFriends:1},
								success: function(data){
									$('head').append(data);
								}
							});
						});
						$('.clearAllMsgNotify').click(function(){
							$.ajax({
								type: 'POST',
								url: '/global/checkNotifications.php',
								data: {clearAll:1},
								success: function(data){
									$('head').append(data);
								}
							});
						});
						$('.unreadAll').click(function(){
							var allN_id = $(this).attr("n_id");
							var n_type = $(this).attr("n_type");
							var n_link_id = $(this).attr("n_link_id");
							var shoutText = $('[n_id='+allN_id+'] .notificationContainer a span').text();
							$.ajax({
								type: 'POST',
								url: '/global/checkNotifications.php',
								data: {clearGeneral:allN_id,n_type:n_type,n_link_id:n_link_id,text:shoutText},
								success: function(data){
									$('head').append(data);
								}
							});
						});
						$('.clearAllGeneral').click(function(){
							$.ajax({
								type: 'POST',
								url: '/global/checkNotifications.php',
								data: {clearAllGeneral:1},
								success: function(data){
									$('head').append(data);
								}
							});
						});
						$('.acceptdeny').click(function(){
							if ($(this).attr("class") == "friendAccept acceptdeny"){
								var friendAccept = true;
							}else{
								var friendAccept = false;
							}
							var friendID = $(this).attr("b_id");
							$.ajax({
								type: 'POST',
								url: '/global/checkNotifications.php',
								data: {reqID:friendID,friendAccept:friendAccept},
								success: function(data) {
									$('head').append(data);
								}
							});
						});
												$(document).on('click', '.podcast-live-hide-btn', function(){
							hidePodcastEmbed();
							// setHiddenPodcastCookie();
						});
						$(document).on('click', '.podcast-live-link', function(event){
							hidePodcastEmbed();
							// setHiddenPodcastCookie();
						});
						$('.podcast-link-live-btn').on('click', function(event){
							event.preventDefault();
							deleteCookie('hide_live_podcast');
							showPodcastEmbed();
						}); 
						//element will still exist if the podcast has been closed
						if($('.podcast-embed-iframe').length>0){
							setHiddenPodcastCookie();
						}
					});
				</script>
