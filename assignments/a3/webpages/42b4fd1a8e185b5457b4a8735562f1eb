  <script type="text/javascript">
    $("body").css("visibility", "hidden");
    var headerUrl = 'https://www.unrealengine.com/unifiedheader';
    // IE8 & 9 only Cross domain JSON GET request
    if ('XDomainRequest' in window && window.XDomainRequest !== null) {
        var xdr = new XDomainRequest(); // Use Microsoft XDR
        xdr.open('get', headerUrl);
        xdr.onload = function () {
            appendData(xdr.responseText);
            successCallback(JSON); // internal function
        };

        xdr.onerror = function() {
            _result = false;
        };

        xdr.send();
    }else {
        $.ajax({
            cache: true,
            url: headerUrl,
            success: function (data) {
                appendData(data);
            }
        });
    }

    function appendData(data){
        $("#unified_header").prepend(data);


            var loginLink="/users/login.html";
            $("#btn-signin").css({"display":"inline-block"});
            $(".sso-sign-in").attr("href", loginLink);

            // Remove the sso-sign-in class so the onclick js is not invoked
            $(".sso-sign-in").removeClass("sso-sign-in");
    }

    window.onload = function () {
        $("body").css("visibility","visible");
    }
</script>  
