<script type="text/javascript">
	var isLogin = 'false';
	var isAdultWebtoon = 'N';
	var isOver19 = 'false';
	var isParty = 'true';
	var levelName = "challenge";
	
	var artistData = [
       
          {"artistId" : 297401,
           "nickname": 'fffiesta'}
           
    ];

    var actionRunner;


    WebtoonArtistActionRunner = $Class ({
        $init : function(titleId) {
            this._titleId = titleId;
	    },

	    artistForFavorite : function(element) {
	    	$Element("author_layer_inner").html("");
	    	$Element("author_layer").css("border", "");
            $Element("author_layer_inner").css("border", "1px solid #b6b6b6");
	    	for (var i = 0; i < artistData.length; i++) {
		    	var liElement = new $Element("<li>");
		    	var aElement = new $Element("<a>");
		    	aElement.attr("href", "javascript:actionRunner.addFavoriteArtist('" + artistData[i].artistId + "')");
		    	aElement.html("관심작가 등록&lt;" + artistData[i].nickname + "&gt;")

		    	liElement.append(aElement);
		    	
		    	$Element("author_layer_inner").append(liElement);

                if (i==9) {
                    $Element("author_layer").css("border", "1px solid #b6b6b6");
                    $Element("author_layer_inner").css("border", "0");
                }
	    	}
		    artistLayer.show(element);
	    },
	  	//관심 웹툰 추가
        addFavoriteWebtoon : function (titleId) {
	    	var width = (screen.width / 2) - 800;
			var height = (screen.height / 2) - 510;
		    if (!false) {
			    if(confirm('로그인 해야 이용할 수 있습니다. 로그인 하시겠습니까?')) {
			    	window.open("https://nid.naver.com/nidlogin.login?svctype=64&url=http://comic.naver.com/mypage/favoriteArtist.nhn?m=insert%26titleId="+titleId,"addFavorite" ,"directories=no,toolbar=no,scrollbars=no,width=370,height=180,top="+height+",left="+width);
				}
		    } else {
				window.open("/mypage/favoriteArtist.nhn?m=insert&titleId="+titleId,"addFavorite" ,"directories=no,toolbar=no,scrollbars=no,width=370,height=180,top="+height+",left="+width);
		    }
	    },
	    
	    artistForArtistTitle : function(element) {
			$Element("author_layer_inner").html("");
			$Element("author_layer").css("border", "");
			$Element("author_layer_inner").css("border", "1px solid #b6b6b6");
	    	if(artistData.length == 0) {
	    		var liElement = new $Element("<li>");
                var spanElement = new $Element("<span>");
                spanElement.html("작가의 다른 작품이 없습니다.");
                liElement.append(spanElement);
                $Element("author_layer_inner").append(liElement);
	        	artistLayer.show(element);
	    	} else if(artistData.length == 1) {
	    		location.href = "/artistTitle.nhn?artistId=" + artistData[0].artistId;	    		
	    	} else {
					for (var i = 0; i < artistData.length; i++) {
						var liElement = new $Element("<li>");
						var aElement = new $Element("<a>");
						aElement.attr("href", "/artistTitle.nhn?artistId=" + artistData[i].artistId);
						aElement.html("&lt;" + artistData[i].nickname + "&gt;의 다른작품");
						liElement.append(aElement);
						$Element("author_layer_inner").append(liElement);
						if (i==9) {
							$Element("author_layer").css("border", "1px solid #b6b6b6");
							$Element("author_layer_inner").css("border", "0");
						}
	              }
	        artistLayer.show(element);
	    	}
	    	return false;
	    },
	     
	    addFavoriteArtist : function(artistId) {
		    if (!isLogin) {
			    if( confirm('로그인 해야 이용할 수 있습니다. 로그인 하시겠습니까?') ) {
			        location.href = "https://nid.naver.com/nidlogin.login?mode=form&url="+encodeURIComponent(location.href);
                }
		    } else {
		        location.href = '/mypage/favoriteArtist.nhn?m=insert&artistId=' + artistId +
		                        '&titleId=' + this._titleId + '&nextUrl=' + encodeURIComponent(location.href)  ;
		    }
                
	    }
	});
			
	
	function loadThisWindow () {
	    actionRunner = new WebtoonArtistActionRunner("671120");
	    
		(function(oData) {
			window.__oLikeItData = oData;
			var s = document.createElement("script");
			s.type = "text/javascript";
			s.charset = "utf-8";
			s.src = oData.domain + "/static/js/likeIt.v2.desktop.init.js?v="+ (new Date()).toDateString().replace(/\s/g,"");
			(document.head || document.getElementsByTagName("head")[0]).appendChild(s);
		})({
			serviceId : "COMIC"
			, displayId : "COMIC"
			, contentId : "671120"
			, domain :"http://comic.like.naver.com"
			, lang : "ko"
			, viewType : ""
			
		 });

		if(levelName == "bestChallenge") {
			function potenupArea() {
				if ($Element("potenupAreaBtn").className() == "btn_potenup") {
					$Element("potenupAreaBtn").className($Element("potenupAreaBtn").className() + " on");
				} else {
					$Element("potenupAreaBtn").className("btn_potenup");
				}

			}

			$Fn(potenupArea).attach($Element("potenupAreaBtn"), "click");

			$Fn(potenupArea).attach($Element("potenupAreaCloseBtn"), "click");
		}
	}

	var popupObject = new nhn_naver_commcast_feed_popup();  // 팝업처리 객체 생성

	function applyFeed(){
		if(isAdultWebtoon == 'Y'){
			if (isLogin == 'false') {
				location.href = "https://nid.naver.com/nidlogin.login?viewtype=3&svctype=128&url="+encodeURIComponent(location.href);
			}else if ( (isLogin == 'true') && (isOver19 == 'false') ){
				location.href = "/"+levelName.toLowerCase()+"/detail.nhn?titleId="+671120+"&no=1";
			}else if ( (isLogin == 'true') && (isParty == 'true') ){
				location.href = "/"+levelName.toLowerCase()+"/detail.nhn?titleId="+671120+"&no=1"; 
		    }else{
		    	var params = {
	  			  popupType   : 'C',
	  			  callbackUrl  : '',
	  			  serviceId   : 'webtoon',
	  			  feedGroupId  : 'comic',
	  			  feedGroupName  : 'comic',
	  			  feedId    : ['671120'],
	  			  feedName   : ["Cafe &#039;OK&#039;"],
	  			  feedUrl   : ["http://comic.naver.com/challenge/list.nhn?titleId=671120"],
	  			  selected   : ['Y']
	  			};
	  			//popupObject.popupFeedWindow(params);
	  			popupObject.popupFeedWindow(params,{charEnc:'UTF-8',feedPopupHost:'relation.commcast.naver.com'});  
		    }
		}else{
			var params = {
			  popupType   : 'C',
			  callbackUrl  : '',
			  serviceId   : 'webtoon',
			  feedGroupId  : 'comic',
			  feedGroupName  : 'comic',
			  feedId    : ['671120'],
			  feedName   : ["Cafe &#039;OK&#039;"],
			  feedUrl   : ["http://comic.naver.com/challenge/list.nhn?titleId=671120"],
			  selected   : ['Y']
			};
			//popupObject.popupFeedWindow(params);  
			popupObject.popupFeedWindow(params,{charEnc:'UTF-8',feedPopupHost:'relation.commcast.naver.com'});
		}		
	}
		
	$Fn(loadThisWindow).attach(window,"load");

	/**
	 * 북스토어로 이동 가능한지 확인해서 이동
	 *
	 * @params nbooksId {String} 북스 상품 ID
	 */
	function moveBookstore(nbooksId) {
		 var result;
		$Ajax('http://nstore.naver.com/isService.json', {
			type : 'jsonp',
			method : 'get',
			onload : function(oRes) {
				result = oRes.json();
			}
		}).request({
			'productTypeCode' : 'COMIC',
			'originalProductId' : nbooksId
		});
		
		
		var oInterval = setInterval(function() {
			if (result) {
				if (result == 'true') {
					window.open('http://nstore.naver.com/comic/detail.nhn?originalProductId=' + nbooksId, '_blank');
				} else if (result == 'false') {
					alert('N스토어에 상품을 등록 중입니다. 잠시만 기다려주세요.');
				}
				clearInterval(oInterval);
				oInterval = null;
			}
		}, 100);
	}



