                   amznads.getAdsCallback('3266', function (){
                       amznads.setTargetingForGPTAsync('amznslots');
                        googletag.cmd.push(function(){
                            googletag.pubads().refresh([__tnt.ads.dfp.slots['fixed-big-ad-gallery'].dfp_slot]);
                        });
                       googletag.pubads().clearTargeting('amznslots');
                   });}
                /* reloadByIds will return false/0 if the ad slot called for hasn't actually been loaded to the page. */
                if( typeof __tnt.ads.oas != 'undefined' ) bIsOAS = !!__tnt.ads.oas.reloadByIds(['fixed-big-ad-gallery']);
            
                if( !bOnlyDFP && !bIsOAS && typeof TNCMS.AdManager != 'undefined' ){
                        TNCMS.AdManager.refresh({region: 'fixed-big-ad-gallery'});}
            } catch(e) {
            __tnt.log(e);
        }
                                gallery_ad_8f638e11_7283_5d2a_aea7_e99da79f6bf6 = true;
                                /** set instance of carousel containing active ad slide. */
                                $sGalleryAdInView = '#gallery-items-8f638e11-7283-5d2a-aea7-e99da79f6bf6';
                            }
                            if( gallery_changed_8f638e11_7283_5d2a_aea7_e99da79f6bf6 ){
                                
                            }
                            gallery_changed_8f638e11_7283_5d2a_aea7_e99da79f6bf6 = true;
                        

                        
                    },
                    onTranslated:function(){
                        
                        
                        
                            __tnt.trackEvent($('#gallery-items-8f638e11-7283-5d2a-aea7-e99da79f6bf6 .photo-container').eq(this._current).data('track-object'));
                        

                        
                        
                            var currHash = $('#gallery-items-8f638e11-7283-5d2a-aea7-e99da79f6bf6 .photo-container').eq(this._current).data('count');
                            if(currHash){
                                window.location.hash = currHash;
                            }
                        
                    }
                });

                
                var photo_carousel_8f638e11_7283_5d2a_aea7_e99da79f6bf6 = $('#gallery-items-8f638e11-7283-5d2a-aea7-e99da79f6bf6').data('owlCarousel');

                $('#carousel-8f638e11-7283-5d2a-aea7-e99da79f6bf6 .photo-carousel-control.left').on(__tnt.client.clickEvent, function(e){
                    photo_carousel_8f638e11_7283_5d2a_aea7_e99da79f6bf6.prev();
