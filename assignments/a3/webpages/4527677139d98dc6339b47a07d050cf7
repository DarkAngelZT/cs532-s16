<script type="text/javascript" async="" src="/themes/js/comments.js"></script>
<script type="text/javascript">
    $(function() {
        var offset = $(".c-center").offset();
        var height = $(".c-center").height();
        var c_right_height = $(".c-right").height();
        var c_right_width = $(".c-right").width();
        var c_right_offset = $(".c-right").offset();
        var content_height = $(".content > .block-def").height();
        $(".c-right").css('height', content_height);


        $(window).scroll(function() {
            //var block_content_height = $(".content").height();
            if ($(window).scrollTop()> (offset.top + height)) {
                $(".c-center").addClass('hide');
                if (!$(".c-under").hasClass('w75')) {
                    $(".c-under-wrapper").css('width', $(".c-under").width());
                }

                $(".c-under").addClass('w75');
            }
            else {
                $(".c-center").removeClass('hide');
                $(".c-under-wrapper").css('width', '100%');
                $(".c-under").removeClass('w75');
            };

            var tmp = 0;
            /* Если правая колонка меньше */
            if (c_right_height < content_height) {

                if ($(window).scrollTop()> (c_right_offset.top + c_right_height - $(window).height())) {

                    if ($.browser.chrome) {
                        tmp = 120;
                    }
                    else {
                        tmp = 0;
                    }

                    if ($(window).scrollTop()> (content_height + tmp)) {
                        $(".c-right-wrapper").css('position', 'absolute');
                        $(".c-right-wrapper").css('bottom', '10px');
                        $(".c-right-wrapper").css('width', '');
                    }
                    else {
                        $(".c-right-wrapper").addClass('fixed');
                        $(".c-right-wrapper").css('position', 'fixed');
                        $(".c-right-wrapper").css('bottom', '20px');
                        $(".c-right-wrapper").css('width', c_right_width);
                    }

                }
                else {
                    $(".c-right-wrapper").removeClass('fixed');
                    $(".c-right-wrapper").css('position', '');
                    $(".c-right-wrapper").css('bottom', '');
                    $(".c-right-wrapper").css('width', '');
                }

            }



        }); /**/



    });
</script>

<!-- Переключалка - Комментарии/Чат -->
    <!-- Форма авторизации (class="hide" - скрыть блок)-->
