	   $(function(){
	   //Counter
	      var entry_id = 57861;
	      var script = document.createElement('script');
	      script.type = 'text/javascript';
	      script.async = true;
	      script.src = 'http://appx.sanook.com/counter/set?site_name=movie'+'&entry_id=' + entry_id;
	      document.body.appendChild(script);
	   });
	   function __parseJSONPResponse(data) {$('#ctrt_count').html(addCommas(data.total));}
	   function addCommas(nStr){
	      nStr += '';
	      x = nStr.split('.');
	      x1 = x[0];
	      x2 = x.length > 1 ? '.' + x[1] : '';
	      var rgx = /(\d+)(\d{3})/;
	      while (rgx.test(x1)) {
	         x1 = x1.replace(rgx, '$1' + ',' + '$2');
	      }
	      return x1 + x2;
	   }
	   $('.show-cont-stat').last().after($('div.share-button').show());
	 </script> 

    
    





    

