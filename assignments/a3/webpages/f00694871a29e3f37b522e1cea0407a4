<script type="text/javascript">
  var siteSection = "";
  var siteSectionPCName = "Deportes";
  var siteSectionCCName = "Eishockey";
  var siteSectionGCCName = "NHL";
  
  switch(siteSectionPCName) {
    case "Sports":
    siteSection = "Sports (Other)";
    
      if(siteSectionCCName == "Baseball" && siteSectionGCCName == "MLB") siteSection = "Sports (Main)";
      if(siteSectionCCName == "Basketball" && siteSectionGCCName == "College Basketball") siteSection = "Sports (Main)";
      if(siteSectionCCName == "Basketball" && siteSectionGCCName == "NBA") siteSection = "Sports (Main)";
      if(siteSectionCCName == "Boxing") siteSection = "Sports (Main)";
      if(siteSectionCCName == "Football" && siteSectionGCCName == "College Football") siteSection = "Sports (Main)";
      if(siteSectionCCName == "Football" && siteSectionGCCName == "NFL") siteSection = "Sports (Main)";
      if(siteSectionCCName == "Hockey" && siteSectionGCCName == "NHL") siteSection = "Sports (Main)";
      if(siteSectionCCName == "Mixed Martial Arts") siteSection = "Sports (Main)";
      if(siteSectionCCName == "Soccer") siteSection = "Sports (Main)";
      if(siteSectionCCName == "Tennis") siteSection = "Sports (Main)";
      if(siteSectionCCName == "Wrestling") siteSection = "Sports (Main)";
      
      break;
    case "Concerts":
    case "Theater":
    case "Other":
    default:
      siteSection = siteSectionPCName;
      break;
  }
  
