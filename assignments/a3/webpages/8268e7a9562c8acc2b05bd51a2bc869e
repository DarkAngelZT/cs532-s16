<script>
$(function() {
	$('.tip-this').qtip({
		position: {
			at: 'top center',
			my: 'bottom center'
		},
		content: {
			attr: 'title'
		},
		style: {
			classes: 'qtip-clean2 qtip-shadow',
			tip: {
				corner: true,
				width:20,
				height:10
			}
		}
	});
	$('.qtip-this').qtip({
		position: {
			at: 'left center',
			my: 'right center'
		},
		content: {
			attr: 'title'
		},
		style: {
			classes: 'qtip-clean2 qtip-shadow',
			tip: {
				corner: true,
				width:20,
				height:10
			}
		}
	});
	$(".fancybox").fancybox();
	$(document).on('click','.like',function(e){
		e.preventDefault();
		$.ajax({
			url:$(this).attr('href'),
			type:'GET',
			dataType: 'JSON',
			context: this,
			success: function(json){
				if (json.stat){
					if($('.like-me').hasClass('light-up')){
						$('.like-me').removeClass('light-up');
					}else{
						$('.like-me').addClass('light-up');
					}
					$('.like-count').html(json.likeCount);
				}
			}
		});
	});
});
</script>
