var max = -1;
if (offset > 1) {
$("#content-body-16194260-59055203").children().slice(0, offset - 1).remove();
}
if (max > -1) {
$("#content-body-16194260-59055203").children().slice(max).remove();
}
// Show dateline field at the beginning of the first paragraph
