    <span class="a-declarative" data-action="a-modal" data-a-modal="{&quot;cache&quot;:&quot;false&quot;,&quot;width&quot;:&quot;480&quot;,&quot;name&quot;:&quot;swf&quot;,&quot;header&quot;:&quot;Share this item via Email&quot;,&quot;popoverLabel&quot;:&quot;Share this item via Email&quot;,&quot;url&quot;:&quot;/gp/pdp/taf/dpPop.html/ref=cm_sw_p_view_dp_aE7Wwb1A0CSHX?ie=UTF8&amp;parentASIN=B0184REFK6&amp;tt=sh&amp;referer=http%253A%252F%252Fwww.amazon.com%252Fgp%252Fproduct%252FB0184REFK6%252Fref%253D&amp;placementID=dp_aE7Wwb1A0CSHX&amp;emailTemplate=%2Fgp%2Fpdp%2Ftaf%2Femail%2Fshare-product&amp;contentID=B0184REFK6&amp;isDynamicSWF=0&amp;relatedAccounts=amazondeals%2Camazonmp3&amp;forceSprites=1&amp;contentName=item&amp;ra=taf&amp;viaAccount=amazon&amp;contentURI=%2Fdp%2FB0184REFK6&amp;isEmail=0&amp;id=B0184REFK6&amp;contentType=asin&amp;isDPX=1&amp;redirectLocation=dp%2FB0184REFK6%2F%23_swftext_Swf&quot;}">
        <a id="swfText" class="a-link-normal email" title="Share via e-mail" href="/gp/pdp/taf/ref=cm_sw_l_view_dp_aE7Wwb1A0CSHX?ie=UTF8&amp;parentASIN=B0184REFK6&amp;referer=http%253A%252F%252Fwww.amazon.com%252Fgp%252Fproduct%252FB0184REFK6%252Fref%253D&amp;placementID=dp_aE7Wwb1A0CSHX&amp;emailTemplate=%2Fgp%2Fpdp%2Ftaf%2Femail%2Fshare-product&amp;contentID=B0184REFK6&amp;isDynamicSWF=0&amp;relatedAccounts=amazondeals%2Camazonmp3&amp;itemInfo=B0184REFK6&amp;type=SH&amp;contentName=item&amp;viaAccount=amazon&amp;contentURI=%2Fdp%2FB0184REFK6&amp;contentType=asin&amp;isDPX=1">Share</a><span class="a-letter-space"></span>
      <a id="swfImage" class="a-link-normal email" title="Share via e-mail" href="/gp/pdp/taf/ref=cm_sw_l_view_dp_aE7Wwb1A0CSHX?ie=UTF8&amp;parentASIN=B0184REFK6&amp;referer=http%253A%252F%252Fwww.amazon.com%252Fgp%252Fproduct%252FB0184REFK6%252Fref%253D&amp;placementID=dp_aE7Wwb1A0CSHX&amp;emailTemplate=%2Fgp%2Fpdp%2Ftaf%2Femail%2Fshare-product&amp;contentID=B0184REFK6&amp;isDynamicSWF=0&amp;relatedAccounts=amazondeals%2Camazonmp3&amp;itemInfo=B0184REFK6&amp;type=SH&amp;contentName=item&amp;viaAccount=amazon&amp;contentURI=%2Fdp%2FB0184REFK6&amp;contentType=asin&amp;isDPX=1"><i class="a-icon a-icon-share-email"></i></a><span class="a-letter-space"></span>
