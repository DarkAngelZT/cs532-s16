            jQuery(document).ready(function($){
                var gallery_ad_74b668e8_cc84_11e5_bb51_c3625b9868a2 = false;
                var gallery_changed_74b668e8_cc84_11e5_bb51_c3625b9868a2 = false;
                $('#gallery-items-74b668e8-cc84-11e5-bb51-c3625b9868a2').owlCarousel({
                    items: 1,
                    autoHeight: true,
                    lazyLoad: true,
                    loop: false,
                    navRewind: true,
                    responsiveRefreshRate: 500,
                    
                    
                    dots : false,
                    onChanged: function(){
                        
                        try {
            
            var bOnlyDFP = false, bIsOAS = false;
            if( typeof __tnt.ads.dfp != 'undefined' ){
                        googletag.cmd.push(function(){
                            googletag.pubads().clear();
                            googletag.pubads().refresh();
                        });}
                /* reloadByIds will return false/0 if the ad slot called for hasn't actually been loaded to the page. */
                if( typeof __tnt.ads.oas != 'undefined' ) bIsOAS = !!__tnt.ads.oas.reloadByIds();
            
                if( !bOnlyDFP && !bIsOAS && typeof TNCMS.AdManager != 'undefined' ){
                        TNCMS.AdManager.refresh({region: ''});}
            } catch(e) {
            __tnt.log(e);
        }

                        
                    },
                    onTranslated:function(){
                        
                        
                        
                            __tnt.trackEvent($('#gallery-items-74b668e8-cc84-11e5-bb51-c3625b9868a2 .photo-container').eq(this._current).data('track-object'));
                        

                        
                        
                    }
                });

                
                var photo_carousel_74b668e8_cc84_11e5_bb51_c3625b9868a2 = $('#gallery-items-74b668e8-cc84-11e5-bb51-c3625b9868a2').data('owlCarousel');

                $('#carousel-74b668e8-cc84-11e5-bb51-c3625b9868a2 .photo-carousel-control.left').on(__tnt.client.clickEvent, function(e){
                    photo_carousel_74b668e8_cc84_11e5_bb51_c3625b9868a2.prev();
                });

                $('#carousel-74b668e8-cc84-11e5-bb51-c3625b9868a2 .photo-carousel-control.right').on(__tnt.client.clickEvent, function(e){
                    photo_carousel_74b668e8_cc84_11e5_bb51_c3625b9868a2.next();
                });});
        </script>
                </div>
            

            

            

