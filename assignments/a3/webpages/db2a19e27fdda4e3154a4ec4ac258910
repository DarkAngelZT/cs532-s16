<script type="text/javascript">
	// Custom Variable
	var _ga_jobs = Array("0.小学","1.中学","2.高校","3.専門","4.大学","5.学生","6.公務員","7.経営者","8.事務系","9.技術系","10.会社員","11.業界","12.自営","13.自由","14.専業","15.家事","16.パート","17.無職","18.その他");
	var _ga_user = "No";
	var _ga_sex = "-";
	var _ga_birth = "-";
	var _ga_job = "-";
	var _ga_4gid = domain_getCookie("4GID");
	var _ga_cookie = getCookie("PERSON");
	if( _ga_4gid != ""){
		_ga_user = _ga_4gid;
	}
	if( _ga_cookie != ""){
		var _ga_person = _ga_cookie.split(":");
		if( _ga_person[0] == "V3" ){
			if( _ga_person[1] == "F"){
				_ga_sex = "Female";
			}else if( _ga_person[1] == "M"){
				_ga_sex = "Male";
			}
			if( _ga_person[2] != ""){
				_ga_birth = _ga_person[2];
			}
			if( _ga_person[3] != ""){
				_ga_job = _ga_jobs[_ga_person[3]];
			}
		}
	}

  var _gaq = _gaq || [];
  _gaq.push(['_setAccount', 'UA-769738-1']);
//  _gaq.push(['_setDomainName', '4gamer.net']);

	_gaq.push(['_setCustomVar',1,'Member',_ga_user,1]);
	_gaq.push(['_setCustomVar',2,'Sex',_ga_sex,1]);
	_gaq.push(['_setCustomVar',3,'Birth',_ga_birth,1]);
	_gaq.push(['_setCustomVar',4,'Jobs',_ga_job,1]);

  _gaq.push(['_trackPageview']);

  (function() {
    var ga = document.createElement('script'); ga.type = 'text/javascript'; ga.async = true;
    ga.src = ('https:' == document.location.protocol ? 'https://ssl' : 'http://www') + '.google-analytics.com/ga.js';
    var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(ga, s);
  })();

</script>


