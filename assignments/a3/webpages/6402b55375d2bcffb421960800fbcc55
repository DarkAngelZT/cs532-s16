	<div id="vote">
   
   <script type="text/javascript">
	var show_result = true;
</script><script type="text/javascript">
var OPT_ID = 0;
var OPT_TITLE = 1;
var OPT_VOTES = 2;
var poll_id;
var votedID;

$(document).ready(function(){

  $("#pollForm").submit(formProcess); // setup the submit handler  
  if ($("#pollResults").length > 0 ) {
    animateResults();
  }
  
  if (show_result) {
  	  var pollForm = document.getElementById('pollForm');
	  var poll_id = pollForm.poll.value;
	  if ($.cookie('poll_id') && $.cookie('poll_id') == poll_id) {
		var site_id = pollForm.site_id.value;
		var mode = pollForm.mode.value;
		var XID = '';
		if ( typeof(pollForm.XID) != 'undefined' && pollForm.XID !== null)
			XID = pollForm.XID.value;
		$("#pollBlock").empty();
		poll_id = $.cookie('poll_id');
		var rs = randomFromInterval(10000, 99999);
		$.getJSON("/share/php/eepoll.php?vote=none"+"&poll="+poll_id+"&site_id="+site_id+"&mode="+mode+"&XID="+XID + "&rs=" + rs,loadResults);
	  }else{ 
	  	  var poll_html = '<div id="poll_render_form" style="display:none;"><div class="pollBlock" id="pollBlock"><form method="post" action="" id="pollForm_hide">';
		  poll_html = poll_html + ''+ $("form#pollForm").html() +'</form></div></div>';
		  $(poll_html).insertAfter("div.pollWrapper");
	}
  }
});

function formResult(){
  var pollForm = document.getElementById('pollForm');
  var site_id = pollForm.site_id.value;
  var zone = pollForm.zone.value;
  var poll = pollForm.poll.value;
  var mode = pollForm.mode.value;
  var XID = '';
  if ( typeof(pollForm.XID) != 'undefined' && pollForm.XID !== null)
	XID = pollForm.XID.value;
  $("#pollBlock").fadeOut("slow",function(){
    $(this).empty();
    var rs = randomFromInterval(10000, 99999);
    $.getJSON("/share/php/eepoll.php?vote=none"+"&poll="+poll+"&site_id="+site_id+"&mode="+mode+"&zone="+zone+"&XID="+XID + "&rs=" + rs,loadResults);
    });
  return false;
}

function formProcess(event){
  event.preventDefault();
  var pollForm = document.getElementById('pollForm');
  var site_id = pollForm.site_id.value;
  var zone = pollForm.zone.value;
  var poll_id = pollForm.poll.value;
  var mode = pollForm.mode.value;
  var XID = '';
  if ( typeof(pollForm.XID) != 'undefined' && pollForm.XID !== null)
	XID = pollForm.XID.value;
  var id = $("input[@name='vote']:checked").attr("value");
  
  $("#pollBlock").fadeOut("slow",function(){
    $(this).empty();
    votedID = id;
    var rs = randomFromInterval(10000, 99999);
    $.getJSON("/share/php/eepoll.php?vote="+id+"&poll="+poll_id+"&site_id="+site_id+"&mode="+mode+"&zone="+zone+"&XID="+XID+ "&rs=" + rs,loadResults);
    $.cookie('vote_id', id, {expires: 1});
    $.cookie('poll_id', poll_id, {expires: 1});
    });
}

function animateResults(){
  $("#pollResults div").each(function(){
      var percentage = $(this).next().text();
      $(this).css({width: "0%"}).animate({
				width: percentage}, 'slow');
  });
}

function loadResults(data) {
  var total_votes = 0;
  var percent;
  
  for (var i=0; i<data.length; i++) {
    total_votes = total_votes+parseInt(data[i][OPT_VOTES]);
  }
  
  var results_html = "<div class='pollQuestion'>หนังเรื่องใดที่คุณอยากดูที่สุดในปี 2014</div><div id='pollResults'>";
  results_html = results_html+"<ul>";

  var count_percent = 0;
  for (var i=0; i<data.length; i++) {
  	percent = Math.round((parseInt(data[i][OPT_VOTES])/parseInt(total_votes))*100);

  	if(i == (data.length-1)){
  		if(count_percent > 0){
  			percent = 100 - count_percent;
  		}
  	}else{
  		count_percent += percent;
  	}

    if (data[i][OPT_ID] != votedID) {
      results_html = results_html+"<li class='pollAnswer'>"+data[i][OPT_TITLE]+" ("+percent+"%)<div id='bar"+data[i][OPT_ID]+"'style='width:0%;background-color:#cc6600;'>&nbsp;</div><strong style='display:none;'>"+percent+"%</strong></li>";

    } else {
      results_html = results_html+"<li class='pollAnswer'>"+data[i][OPT_TITLE]+" ("+percent+"%)<div id='bar"+data[i][OPT_ID]+"'style='width:0%;background-color:#0066cc;'>&nbsp;</div><strong style='display:none;'>"+percent+"%</strong></li>";

    }
  }
  results_html = results_html+"</ul>";  
  results_html = results_html+"</div><div class='pollTotalVotes'>ผู้ร่วมโหวต: "+total_votes+"</div>";

  if(! $.cookie('poll_id')){
	  results_html = results_html+"<div><input id='btnPollBack' type='button' value='โหวต' onclick='poll_fadeout();' /></div>";
  }
  
  $("#pollBlock").append(results_html).fadeIn("slow",function(){
    animateResults();});
}


function poll_fadeout(){
	$("#pollBlock").fadeOut("slow", function(){
			$("div.pollWrapper").html($("#poll_render_form").html());
			$("#pollForm_hide").attr('id', 'pollForm');
			$("#pollForm").submit(formProcess);
			//alert($("div.pollWrapper").html());
	});
	return false;
}

function randomFromInterval(from, to){
    return Math.floor(Math.random()*(to-from+1)+from);
}
