        <form method="post" data-uuid="74b668e8-cc84-11e5-bb51-c3625b9868a2" class="save-list-74b668e8-cc84-11e5-bb51-c3625b9868a2-form save-asset-to-list" action="/users/admin/list/">
        <input type="hidden" name="action" value="addasset" />
        <input type="hidden" name="format" value="json" />
        <input type="hidden" name="listasset" value="74b668e8-cc84-11e5-bb51-c3625b9868a2" />
        <input type="hidden" name="listapp" value="editorial" />
        <input type="hidden" name="autocreate" value="true" />
        <input type="hidden" name="listname" value="articles" />
