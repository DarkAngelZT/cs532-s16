     <script type="text/javascript">
        $(function() {
            $('.edit-log-time').click(function(evt) {
                var log = $(this).attr('name');
                var $log = $(this).parent();

                var hours = $log.find('.work-log-hours').html();
                var minutes = $log.find('.work-log-minutes').html();

                if (!hours) {
                    hours = '00';
                }

                if (hours.length < 2) {
                    hours = '0' + hours;
                }

                if (!minutes) {
                    minutes = '00';
                }

                if (minutes.length < 2) {
                    minutes = '0' + minutes;
                }

                var observations = $log.find('pre').html();

                getWorkLog(evt, false, false, function(time, observations, ma) {
                    $.getJSON('/epic/editSupportLog.json', {
                        'log': log,
                        'time': time,
                        'observations': observations
                    }, function() {
                        commandUtils.reloadPage();
                    });
                }, hours + ':' + minutes, observations, true);
            });
        });
    </script> 
