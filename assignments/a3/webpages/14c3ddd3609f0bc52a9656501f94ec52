<script>
		     $(function() {
		        newsletter_signup_validate()
		      });
		
		     function newsletter_signup_validate() {
		        $.validator.addMethod("noSpace", function(value, element) {
		            return value.indexOf(" ") < 0 && value != "";
		        }, "No space please and don't leave it empty");
		
		        $.validator.addMethod("custom_chracters", function(value, element) {
		            return this.optional(element) || value.match(/^[0-9,.\^A-Za-z-, \\&/]+$/);
		        }, "Please avoid this character");
		
		        $.validator.addMethod("Alphabets", function(value, element) {
		            return this.optional(element) || value.match(/^[0-9.A-Za-z \-.]+$/);
		        }, "Please avoid this special characters except . and -");
		
		        $.validator.addMethod("companyname", function(uid, element) {
		            return (this.optional(element) || uid.match(/[^\d,0-9]+/));
		        }, "Please avoid only numbers and special characters except . and -");
		        $.validator.addMethod("emailordomain", function(value, element) {
		  		return this.optional(element) || /^\w+@[a-zA-Z_]+?\.[a-zA-Z]{2,3}$/.test(value) || /[-a-zA-Z0-9@:%_\+.~#?&//=]{2,256}\.[a-z]{2,4}\b(\/[-a-zA-Z0-9@:%_\+.~#?&//=]*)?/.test(value);
			},"please enter correct domain");
		        $('#newsletter_signup').validate({
		            rules : {
		                        "questionnaire_submission[new_answers][TextAnswer][613a6f7f-ad59-4fb1-9190-42a2a6ae4d24]" : {
		                            required : true,
		                            Alphabets : true                       
		                        },
		                        "questionnaire_submission[new_answers][TextAnswer][e24d9367-8903-462f-9326-5815e35d106d]" : {
		                            required : true,
		                            email : true,
		                       },
		                       "recaptcha_response_field" : {
                          required : true,
                        }
		                    },
		            messages : {
		                "questionnaire_submission[new_answers][TextAnswer][613a6f7f-ad59-4fb1-9190-42a2a6ae4d24]" : {
		                            required : "Name should not be blank?",
		                            Alphabets : "Please enter alphabet characters."                      
		                        },
		                        "questionnaire_submission[new_answers][TextAnswer][e24d9367-8903-462f-9326-5815e35d106d]" : {
		                            required : "Email field should not be blank?",
		                            email : "Invalid Email id"
		                       },
		                "recaptcha_response_field" : {
                          required : "Please fill the captcha field",
                        }
		            },
		        });
		    }
</script>

<script type="text/javascript">

    $(document).on('click','#newsletter_signin_form_submit',function(event){
  event.preventDefault();
if($('#newsletter_signup').valid()){
    var email = $('#questionnaire_submission_new_answers_TextAnswer_e24d9367-8903-462f-9326-5815e35d106d').val();
    $.ajax({
          url:'/wps/questionnaires/save_questionnaire_form/f6de85fe-f05c-4829-81df-4fabfef59135',
          type:'POST',
          data:$('#newsletter_signup').serialize(),
          dataType:'JSON',
          beforeSend : function(xhr) {
            xhr.setRequestHeader('X-CSRF-Token', $('meta[name="csrf-token"]').attr('content'))
          },
          success:function(data) {
              console.log(data)
              if (data.status == 'success'){
              	$('.newsletter_popup').html("<h1>Thank you for subscribing.</h1>");
              }else{
              $('#recaptcha_tags_error').html("Captcha image Didn't Match,Please enter again");
              showRecaptcha();
              }
        }
      });
}
});

</script>

		<script>
			$(function() {
				newsletter_signup_validate()
				$("#newsletter_form").validate({
					rules : {
						"email" : {
							required : true,
							email : true,
							emailordomain : true,
						},
						"name-field" : {
							required : true,
							Alphabets : true
						}
					},
					messages : {
						"email" : {
							required : "Please enter a email address",
							email : "Please enter a valid email address",
							emailordomain : "Please enter email correct domain",
						},
						"name-field" : {
							required : "Please enter the name",
							Alphabets : "Please enter alphabet characters."
						}
					}
				});
				$('#newsletter_form').validate();
			});
			$(document).on('click', '#newsletter_email', function(event) {
				event.preventDefault();
				if ($("#newsletter_form").valid()) {
					$('#questionnaire_submission_new_answers_TextAnswer_e24d9367-8903-462f-9326-5815e35d106d').attr('value', $('#newsletter_part').val())
					$('#questionnaire_submission_new_answers_TextAnswer_613a6f7f-ad59-4fb1-9190-42a2a6ae4d24').attr('value', $('#name-field').val())
					$("#newsletter-head").dialog({
						dialogClass : 'news_letter_subscriper',
						modal : true
					});
					$('#newsletter_login .single-field input.input_style').val('')
				}
			});
		</script>

		<script type="text/javascript">
			$(document).ready(function() {
				$('#gallery-block').flexslider({
					animation : "fade",
					slideshow : true,
					pauseOnHover : true,
					autoPlay : true,
					controlNav : true,
					animationLoop : true,
					start : function() {
						$(".flex-prev").attr("title", "Previous");
						$(".flex-next").attr("title", "Next");
					}
				});
			});
		</script>
		<script type="text/javascript">
			$(function() {
				$(".event-triger li a").click(function() {
					var a = $(this).attr("data-id");
					$(".event-triger li").removeClass("active");
					$(this).parents("li").addClass("active");
					$(".read-coment").hide();
					$("#" + a).show();
				});
			});
		</script>
		<script type="text/javascript">
			$(document).ready(function() {
				$(".fullscreen").click(function() {

					if (screenfull.enabled) {
						screenfull.toggle($('#gallery-block')[0]);
					} else {
						if ($('.gallery-block').hasClass('fullgallery')) {
							esc_fullscreen()
						} else {
							$(document).scrollTop(0);
							$('.gallery-block').addClass("fullgallery");
							$('.over_lay').css('display', 'block');
							$('body').css('overflow', 'hidden');
						}
						return false;
					}
				});

				var esc_fullscreen = function() {
					$(document).scrollTop(100);
					$('.gallery-block').removeClass("fullgallery");
					$('.over_lay').css('display', 'none');
					$('body').css('overflow', 'scroll')
				}

				$(document).keyup(function(e) {
					if (e.keyCode == 27) {
						esc_fullscreen();
					}
				});

				$(document).on('click', '.over_lay', function() {
					esc_fullscreen();
				});

			});
		</script>
	</body>
</html>
