  var triggered = 0;
  var reviewsSection = A.$("#cm_cr_dpwidget");  
  onScreenTriggerMethod();
  A.on.scroll(onScreenTriggerMethod);
  
  function onScreenTriggerMethod(){
    if (!triggered && A.onScreen(reviewsSection)) {
      triggered = 1;
      A.post('/gp/customer-reviews/aj/customer/ranking-trigger', {
          success: function(data){
          },
 
          error: function(data){
          },

          params: {
            csrfT: '0.364293650511097;1455666135;15b44416cd60a97f5ce9c48349eb8f04275b4c2031cfecc30ee23e21e5025d69',
            requestId: '002YQXYBX8N97F1AH3ZE',
            sessionId: '181-8392723-5910668',
            weblabID: ''
          }
      });
    }
  }

});

</script>








