		var googletag = googletag || {};
		googletag.cmd = googletag.cmd || [];
		function endsWith(str,suffix){
			return str.indexOf(suffix,str.length - suffix.length) !== -1;
		};
		var roadblocks = ["Prem_gd_awards_roadblock_980x120","Prem_gd_awards_roadblock_980x120_Test","Prem_gd_awards_roadblock_980x120_Test","Prem_gd_hardware_roadblock_980x120","Prem_gd_hardware_roadblock_980x120_Test","Prem_gd_home_roadblock_980x120","Prem_gd_home_roadblock_980x120_Test","Prem_gd_home_roadblock_980x120","Prem_gd_home_roadblock_980x120_Test","Prem_gd_therest_roadblock_980x120_Test","Prem_gd_therest_roadblock_980x120","Prem_gd_therest_roadblock_980x120_Test"];googletag.cmd.push(function(){googletag.defineSlot('/1047478/sky160x600', [160,600], 'div-gpt-ad-1389951994225-0').addService(googletag.pubads());googletag.defineSlot('/1047478/TopBanner728x90', [728,90], 'div-gpt-ad-1389951994225-1').addService(googletag.pubads());googletag.defineSlot('/1047478/mpuSmallNew300x250', [300,250], 'div-gpt-ad-1389951994225-19').addService(googletag.pubads());googletag.defineSlot('/1047478/mpuSmall2nd300x250', [300,250], 'div-gpt-ad-1389951994225-20').addService(googletag.pubads());googletag.defineSlot('/1047478/mpuSmallHome300x250', [300,250], 'div-gpt-ad-1389951994225-21').addService(googletag.pubads());googletag.defineSlot('/1047478/bottomBannerNew728x90', [728,90], 'div-gpt-ad-1389951994225-22').addService(googletag.pubads());googletag.defineSlot('/1047478/Prem_gd_therest_halfpage_300x600', [[300, 250], [300, 600]], 'div-gpt-ad-1389951994225-26').addService(googletag.pubads()).setCollapseEmptyDiv(true,true);googletag.defineSlot('/1047478/Prem_gd_therest_roadblock_980x120', [[728,90], [970,66], [980,120], [970,250]], 'div-gpt-ad-1389951994225-28').addService(googletag.pubads());googletag.defineSlot('/1047478/Prem_gd_therest_skin_1x1', [[1920,1527], [1903,1000]], 'div-gpt-ad-1389951994225-30').addService(googletag.pubads());				googletag.on("gpt-slot_rendered",function(e,level,message,service,slot,reference){
					$('#site-focus').css('visibility','visible');
					var slotName = slot.getSlotId();
					for (var key in slotName) if (slotName.hasOwnProperty(key)) break;
						var slotSearch = slotName[key];
					for(var i=0;i<roadblocks.length;i++){
						var search = roadblocks[i];
						if(endsWith(slotSearch,search)){
							var roadblock = document.getElementById(slotName.getDomId());
							var focusEnclose = document.getElementById('focus-enclose');
						// alert(focusEnclose.style.height);
							// alert(roadblock.clientHeight);
							if(roadblock.clientHeight >= 120){
								focusEnclose.style.height = roadblock.clientHeight+'px';
							}else if(roadblock.clientHeight == 66){
						//this is campaign specific not sure how to not do this
						focusEnclose.style.height = roadblock.clientHeight+'px';

							// var alignRoadBlock = document.getElementById('ad-alignment-roadblock');
							// alignRoadBlock.style['vertical-align'] = 'top';
							// alignRoadBlock.style.width = alignRoadBlock.clientWidth;
							// document.getElementById('ad-alignment-float-right').style.width = '100%';
							// roadblock.style.margin = '0 0 0 5px';
						}
					}
				}
			});
				googletag.pubads().enableSingleRequest();
				googletag.enableServices();
			});
