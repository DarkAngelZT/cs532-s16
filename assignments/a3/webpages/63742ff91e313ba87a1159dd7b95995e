<script>
function pageInit() {
  $('.StoryBody ul li, .StoryBody ol li').wrapInner( "<span></span>");
  $('.tipafriend').click(function () {
		$( "#dialog" ).dialog();
  });
	$('#tipform').submit(function () { return libercus.ValidateOK; });
	$('.libAjaxSaveButton').button();
	$('#normalsend').click(function (event) {
		event.preventDefault();
		libercus.AjaxFormValidate($('#tipform'), function () {
			$('#tipform').submit();
		});
	});
	$('#ajaxsend').click(function (event) {
		event.preventDefault();
		var dlg = $(this).parents(".ui-dialog-content:first");
		libercus.AjaxFormValidate($('#tipform'), function () {
			libercus.AjaxSubmit($('#tipform'), '/ugchandler/sendtip', function (result) {
				if (result.ResultCode == 1) {
					// libercus.MessageDialog("Your e-mail has been sent");
					dlg.dialog('close');
				} else {
					libercus.MessageDialog("Error = " + result.ResultCode);
				}
			});
		});
	});
	$('.smallclose').click(function () {
		$(this).parents(".ui-dialog-content:first").dialog('close');
	});
	$('button').button();

	var s = $("#sharebuttons-left");
	var w = $(".storycontentwrapper");
	var pos = w.position(); 
	var orgHeight = s.height()
	var bottom = pos.top + w.height() - orgHeight;

    document.addEventListener("touchmove", SocialHover, false);
	document.addEventListener("scroll", SocialHover, false);
	function SocialHover() {
		pos = w.position(); 
		bottom = pos.top + w.height() - orgHeight;
        var windowpos = $(window).scrollTop();
        if (windowpos >= (pos.top + 70) && windowpos <= bottom) {
            s.css("position","fixed").css("top","35px").css("margin-top","0px");
        } 
		else if(windowpos >= (bottom))
		{
			s.css("position","inherit").css("margin-top", (w.height() - orgHeight) +"px");
		}
		else {
            s.css("position","inherit").css("margin-top","0px");
        }
    };
	$( "#mc img" ).load(function() {
	  	$(".img-caption").width($("#mc img").width());
	});
}
</script>
 
