  <script type="text/javascript">
    $("#flashMessages").hide();
    $(document).ready(function() {
        if(flashMessages.length > 0) {
            for (var message in flashMessages){
                $("#flashMessages").prepend(flashMessages[message]);
            }

            $("body").css("margin-top", "2.3em");
            $("#flashMessages").slideDown("slow");
        }
    });
</script> 
