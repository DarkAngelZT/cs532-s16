    <script type="text/javascript">

        var servercookieName = "::Language::";
        var clientcookieName = "::ClientCookie::";

        function cookies_enabled() {
            return document.cookie.indexOf(servercookieName) != -1;
        }

        function cookies_checked() {
            return document.cookie.indexOf(clientcookieName) != -1;
        }

        function yes_cookies(src) {
            //enable forms, etc.
            //console.log('yes cookies');
            $('input').removeAttr('title').each(function() { $(this).attr('title', $(this).attr('data-title')); });
            $('input, .instagramLogin, .login, form, .loginFacebook').removeAttr('disabled');
            $('.instagramLogin, .login, .loginFacebook').show();
        }
        function no_cookies(src) {
            //disable forms, show alert
            $('input')
                .each(function() { if ($(this).attr('title'))$(this).attr('data-title', $(this).attr('title'));})
                .attr('title', "You must accept cookies to use this site");
            $('input, .instagramLogin, .login, form, .loginFacebook').attr('disabled', true);
            $('.instagramLogin, .login, .loginFacebook').hide();

            //show an alert on mobile devices where tooltip won't be as obvious
            var width = window.innerWidth || document.documentElement.clientWidth;
            if (width <= 760 && src != 'init') {
                alert("You must accept cookies to use this site");
            }

        }

        window.IFRAMESESSION = {
            popup: function (url, windowName, width, height, top, left) {
                var windowName = windowName || 'popup',
                    width = width || 640,
                    height = height || 480,
                    top = top || 150,
                    left = left || (screen.width / 2) - (width / 2),
                    win = window.open(url, windowName, 'width=' + width + ',height=' + height + ',top=' + top + ',left=' + left);
                return win;
            },
            sessionComplete: function(){
                setTimeout(function(){
                    if(!window.cookies_enabled())
                    {
                        window.no_cookies('jscheck');
                        $("#cookieGate-IframeSession").hide();
                        $("#siteContainer").show();
                    }
                    else
                    {
                        window.yes_cookies();
                        $("#cookieGate-IframeSession").hide();
                        $("#siteContainer").show();
                    }
                }, 500);
            }};

        function thirdparty_cookies(src)
        {
            var iframeSession = $("#cookieGate-IframeSession");
            if(!cookies_enabled())
            {
                //Let's try the iframe session.
                $("#siteContainer").hide();
                iframeSession.show();
                iframeSession.find('#sessionLink').click(function () {
                    window.IFRAMESESSION.popup('/en-us/IframeSession_Popup', "", "");
                    return false;
                });
            }
            else
                yes_cookies();
        }


        $(function () {
            no_cookies('init');
            if(top.location != location){
                thirdparty_cookies('init');
            }
            else{
                if (!cookies_enabled()) {
                    no_cookies('jscheck');
                }
                else if (!cookies_checked()) {
                    document.cookie = clientcookieName + "=test;path=/;";
                    $.ajax({
                        url: '/en-US/cookieGate/',
                        type: 'GET',
                        dataType: 'text',
                        timeout: 4000,
                        success: function (data) {
                            //console.log(data);
                            if (data.indexOf("true") < 0) no_cookies('ajaxcheck post');
                            else yes_cookies();
                        },
                        error: yes_cookies
                    });
                }
                else
                    yes_cookies();
            }
        });
    </script>

