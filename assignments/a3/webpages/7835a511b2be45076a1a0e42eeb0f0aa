        <script>
            google.load("visualization", "1", {packages:["corechart"]});

            var poll_9899d25a_b874_11e5_bfae_937a816053cb_cookie = 'poll-voted-9899d25a-b874-11e5-bfae-937a816053cb';
            var poll_9899d25a_b874_11e5_bfae_937a816053cb_voted = $.cookie(poll_9899d25a_b874_11e5_bfae_937a816053cb_cookie);
            var poll_9899d25a_b874_11e5_bfae_937a816053cb_options = {
                legend: {position:'none'},
                width: '100%',
                height: '100%',
                title: 'Which SEC team has chances at a national championship next season?',
                hAxis: {minValue:0},
                colors: ['#9CCF9C', '#9C9ACE', '#9C6563', '#9CCFCE', '#316531', '#969A9C', '#CECF9C', '#313031', '#636531', '#CE9ACE', '#663366', '#633031', '#313063', '#316563']
            };

            function poll_results_9899d25a_b874_11e5_bfae_937a816053cb(bBackBtn){
                $.ajax({
                    type: 'GET',
                    url: '/sports/which-sec-team-has-chances-at-a-national-championship-next/poll_9899d25a-b874-11e5-bfae-937a816053cb.html?action=poll:results',
                    dataType: 'json',
                    success: function(result){
                        var data = [];
                        data.push(['Answer', 'Votes', {role:'style'}]);
                        $.each(result.results,function(i,item){
                            var iNumVotes = item.votes;
                            if( iNumVotes === 0 && item.text === $.cookie(poll_9899d25a_b874_11e5_bfae_937a816053cb_cookie) ){
                                iNumVotes = 1;
                            }
                            data.push([item.text, iNumVotes, item.color]);
                        });
                        $poll_9899d25a_b874_11e5_bfae_937a816053cb_data = google.visualization.arrayToDataTable(data);

                        
                            $poll_9899d25a_b874_11e5_bfae_937a816053cb_chart = new google.visualization.BarChart(document.getElementById('poll-9899d25a-b874-11e5-bfae-937a816053cb-results'));
                        

                        $('#poll-9899d25a-b874-11e5-bfae-937a816053cb-results').show();

                        $poll_9899d25a_b874_11e5_bfae_937a816053cb_chart.draw($poll_9899d25a_b874_11e5_bfae_937a816053cb_data, poll_9899d25a_b874_11e5_bfae_937a816053cb_options);

                        $('#poll-9899d25a-b874-11e5-bfae-937a816053cb-your-vote').html($.cookie(poll_9899d25a_b874_11e5_bfae_937a816053cb_cookie));
                        $('#poll-9899d25a-b874-11e5-bfae-937a816053cb-container').hide();

                        if(bBackBtn){
                            $('#poll-9899d25a-b874-11e5-bfae-937a816053cb-back-vote').removeClass('hide');
                        } else {
                            $('#poll-9899d25a-b874-11e5-bfae-937a816053cb-your-vote-container').show();
                        }
                    }
                });
            }

            var poll_9899d25a_b874_11e5_bfae_937a816053cb_resized;
            jQuery(window).on('resize orientationChanged', function(){
                if( $('#poll-9899d25a-b874-11e5-bfae-937a816053cb-results').is(':visible') ){
                    clearTimeout(poll_9899d25a_b874_11e5_bfae_937a816053cb_resized);
                    var poll_9899d25a_b874_11e5_bfae_937a816053cb_resized = setTimeout(function(){
                        $poll_9899d25a_b874_11e5_bfae_937a816053cb_chart.draw($poll_9899d25a_b874_11e5_bfae_937a816053cb_data, poll_9899d25a_b874_11e5_bfae_937a816053cb_options);
                    }, 400);
                }
            });

            jQuery(document).ready(function($){
                if( poll_9899d25a_b874_11e5_bfae_937a816053cb_voted ) {
                    poll_results_9899d25a_b874_11e5_bfae_937a816053cb();
