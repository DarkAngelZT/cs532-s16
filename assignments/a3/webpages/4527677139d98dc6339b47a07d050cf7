<script type="text/javascript">
    $(function(){
        $('span.nb-err').click(function(e){
            $('div.nb-err-win').css("display", "block");
        });
        $('u.close').click(function(e){
            //$('#err-msg-post').css("display", "none");
            //document.getElementsByClassName('nb-err-win').style.display = 'none';
            $('div.nb-err-win').hide();
            //alert('123');
        });/**/

        $('div.nb-err-win form input[type=submit]').click(function(e){
            e.preventDefault();
            $.ajax({
                type: 'POST',
                url: '/frontend/news/ErrorOnFullPost',
                data: { text: $('div.nb-err-win form textarea[name=text]').val(), link : document.URL },
                success: function(data){
                    $('div.nb-err-win form').hide();
                    $('div.thanks_message_err').show();
                }
            });
            return false;
        });
        $('div.nb-err-win u.close-thks-msg').click(function(e){
            e.preventDefault();
                $('div.nb-err-win').hide();
                $('div.nb-err-win form textarea[name=text]').val('');
                $('div.thanks_message_err').hide();
                $('div.nb-err-win form').show();
            return false;
        });
    });
</script>
