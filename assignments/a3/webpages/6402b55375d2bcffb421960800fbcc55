<script type="text/javascript">
$.ui.autocomplete.prototype._renderItem = function( ul, item){
  var term = this.term.split(' ').join('|');
  var re = new RegExp("(" + term + ")", "gi") ;
  var t = item.label.replace(re,"<strong style='color:#FF7F00;'>$1</strong>");
  return $( "<li></li>" )
     .data( "item.autocomplete", item )
     .append( "<a>" + t + "</a>" )
     .appendTo( ul );
};

	$(function() {
		var cache = {},
			lastXhr;
		$( "#s-search-main" ).autocomplete({
			minLength: 3,
			source: function( request, response ) {
				var term = request.term;
				if ( term in cache ) {
					response( cache[ term ] );
					return;
				}

				lastXhr = $.getJSON( "http://movie.sanook.com/share/get_search_title/", request, function( data, status, xhr ) {
					cache[ term ] = data;
					if ( xhr === lastXhr ) {
						response( data );
					}
				});
			}
		});
	});
</script>

<script type="text/javascript">
$("#main-search").live('submit', function(){
	var search_box_url = "";
	search_box_url = "http://movie.sanook.com/search/";
	if($("#s-search-main").val() != "")
	{
		search_box_url = search_box_url + "all/" + $("#s-search-main").val().replace(/%|>|<|{|}|;/gi,"") + "/"; 
	}
	location.href = search_box_url;
	return false;
});
</script>
