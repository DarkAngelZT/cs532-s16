DARLA_CONFIG.events.AUTO = {"name":"AUTO","sp":"2143969880","ps":{"FILM":{"autoMax":25,"autoIV":1,"autoRT":"80000"},"ESPON":{"autoMax":25,"autoIV":1,"autoRT":"80000"},"TL1":{"autoMax":25,"autoIV":1,"autoRT":"80000"},"SBTN-1":{"autoMax":25,"autoIV":1,"autoRT":"80000"},"SBTN-0":{"autoMax":25,"autoIV":1,"autoRT":"80000"},"WBTN-1":{"autoMax":25,"autoIV":1,"autoRT":"80000"},"WBTN-0":{"autoMax":25,"autoIV":1,"autoRT":"80000"},"BTN3":{"autoMax":25,"autoIV":1,"autoRT":"80000"},"BTN2":{"autoMax":25,"autoIV":1,"autoRT":"80000"},"BTN1":{"autoMax":25,"autoIV":1,"autoRT":"80000"},"NE3-3":{"autoMax":25,"autoIV":1,"autoRT":"80000"},"NE3-2":{"autoMax":25,"autoIV":1,"autoRT":"80000"},"NE3-1":{"autoMax":25,"autoIV":1,"autoRT":"80000"},"NE3-0":{"autoMax":25,"autoIV":1,"autoRT":"80000"},"WPS":{"autoMax":25,"autoIV":1,"autoRT":"80000"},"LDRB2":{"autoMax":25,"autoIV":1,"autoRT":"80000"},"LREC3":{"autoMax":25,"autoIV":1,"autoRT":"80000"},"LREC2":{"autoMax":25,"autoIV":1,"autoRT":"80000"},"LREC":{"autoMax":25,"autoIV":1,"autoRT":"80000"}},"sa":"Keyword_Id=\"Aqib_Talib;NFL_Network;Super_Bowl_ring;Super_Bowl\" \"blog\"=\"nfl-shutdown-corner\" \"type\"=\"blogpost\" ctopid=\"2074500;2078500;2096000;2212000;2253000;1035500\" ctype=\"fn_news;article\" lang=\"en-US\" rs=\"lmsid:a077000000CFoGyAAL;revsp:99add987-dcd1-48ae-b801-e4aa58e4ebd0;lpstaid:995fba7f-282d-3191-8ac4-463e03ed8800\" wiki_topics=\"Aqib_Talib;NFL_Network;Super_Bowl_ring;Super_Bowl\"  can_suppress_ugc=\"1\" FSRVY-0=\"1x1\" LREC3=\"300x250,1x1\" LREC2=\"300x250,1x1\" LREC=\"300x250,1x1\" ESPON=\"120x30\"","sa_orig":"Keyword_Id=\"Aqib_Talib;NFL_Network;Super_Bowl_ring;Super_Bowl\" \"blog\"=\"nfl-shutdown-corner\" \"type\"=\"blogpost\" ctopid=\"2074500;2078500;2096000;2212000;2253000;1035500\" ctype=\"fn_news;article\" lang=\"en-US\" rs=\"lmsid:a077000000CFoGyAAL;revsp:99add987-dcd1-48ae-b801-e4aa58e4ebd0;lpstaid:995fba7f-282d-3191-8ac4-463e03ed8800\" wiki_topics=\"Aqib_Talib;NFL_Network;Super_Bowl_ring;Super_Bowl\"  can_suppress_ugc