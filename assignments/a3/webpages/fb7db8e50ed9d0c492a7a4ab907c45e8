    <script type="text/javascript">
    jQuery("document").ready(function($){
    $('.tabb1 a').click(function(e){
         event.preventDefault();
         document.getElementById('tab-content1').classList.remove('tab-content');
         document.getElementById('tab-content2').classList.add('tab-content');
         document.getElementById('tab-content3').classList.add('tab-content');
     });
     $('.tabb2 a').click(function(e){
         event.preventDefault();
         document.getElementById('tab-content2').classList.remove('tab-content');
         document.getElementById('tab-content1').classList.add('tab-content');
         document.getElementById('tab-content3').classList.add('tab-content');
     });
      $('.tabb3 a').click(function(e){
         event.preventDefault();
         document.getElementById('tab-content3').classList.remove('tab-content');
         document.getElementById('tab-content1').classList.add('tab-content');
         document.getElementById('tab-content2').classList.add('tab-content');
     });
     });

    $('#cssmenu').prepend('<div id="menu-button">Menu</div>');
    $('#cssmenu #menu-button').on('click', function(){
    var menu = $(this).next('ul');
    if (menu.hasClass('open')) {
      menu.removeClass('open');
    } else {
      menu.addClass('open');
    }
});

    </script>

