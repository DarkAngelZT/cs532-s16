<script type="text/javascript">
 
P.when('DynamicIframe').execute(function(DynamicIframe){
	var BookDescriptionIframe = null,
	    bookDescEncodedData = "After%20Adire%2C%20a%20quick%20witted%20dominatrix%2C%20finds%20out%20the%20horrific%20truth%20that%20Davis%26%23x2019%3B%20father%20was%20the%20man%20who%20brutalized%20her%20mother%20all%20those%20years%20ago%2C%20she%20decides%20to%20leave%20behind%20the%20only%20man%20she%26%23x2019%3Bs%20ever%20loved.%20Without%20a%20word%2C%20she%20packs%20her%20bags%20and%20leaves%20Boston%20for%20a%20prestigious%20PR%20position%20in%20Beijing.%20Life%2C%20however%2C%20isn%26%23x2019%3Bt%20the%20same%20without%20Davis.%20Neither%2012-hour-work%20days%20nor%20the%20dazzling%20nightlife%20of%20Beijing%20provides%20the%20escape%20she%20had%20been%20hoping%20for.%0D%3Cbr%20%2F%3E%0D%3Cbr%20%2F%3EFor%20Davis%2C%20meanwhile%2C%20the%20truth%20of%20who%20and%20what%20his%20father%20truly%20is%20devastates%20both%20his%20family%20and%20his%20heart.%20%20His%20only%20hope%20is%20that%20Adire%20will%20come%20to%20realize%20that%20he%20is%20nothing%20like%20his%20father%2C%20but%20rather%2C%20a%20man%20deeply%20in%20love%20with%20her%2C%20a%20man%20who%20would%20forever%20put%20her%20needs%20above%20all%20else.%20%20All%20of%20his%20hopes%20for%20her%20return%20are%20dashed%2C%20however%2C%20when%20arrives%20back%20at%20their%20brown%20stone%20on%20Chestnut%20Street%20only%20to%20see%20a%20For%20Sale%20out%20front.%0D%3Cbr%20%2F%3E%0D%3Cbr%20%2F%3EDavis%20decides%20to%20do%20everything%20in%20his%20power%20to%20regain%20Adire%26%23x2019%3Bs%20trust%20and%20love%20and%20immediately%20boards%20a%20plane%20for%20Beijing.%20Even%20as%20he%20does%20so%2C%20further%20problems%20arise%20in%20Adire%26%23x2019%3Bs%20world%20when%20a%20mysterious%20man%20from%20her%20past%20sudden%20reappears%20and%20offers%20an%20ultimatum%20that%20could%20not%20only%20devastate%20her%20career%2C%20but%20her%20heart%20as%20well.%0D%3Cbr%20%2F%3E%0D%3Cbr%20%2F%3EIn%20a%20love%20story%20of%20consummate%20passion%20and%20mystery%2C%20the%20unanswered%20question%20is%20whether%20Adire%20and%20Davis%20can%20come%20together%20to%20put%20their%20pasts%20behind%20them%20or%20are%20the%20brutal%20realities%20of%20their%20pasts%20just%20too%20much%20to%20overcome%26%23x2026%3B%0D%3Cbr%20%2F%3E",
	    bookDescriptionAvailableHeight,
	    minBookDescriptionInitialHeight = 112,
	    options = {},
	    iframeId = "bookDesc_iframe";
 
    function resizeCallback() {
        // Get the line-height of the iframe
        var iframe = document.getElementById(iframeId);
        var iframeDocument = iframe.contentDocument;
 
        if (true && iframeDocument && iframeDocument.defaultView) {
            // Set the height to the number of lines specified
            var numLines = parseInt(5, 10);
 
            // Get the line-height of the iframe
            var iframeContent = iframeDocument.getElementById("iframeContent");
 
            // Compute the line height
            var lineHeight = iframeDocument.defaultView.getComputedStyle(iframeContent, null).getPropertyValue("line-height");
 
            // Parse the line height
            lineHeight = parseFloat(lineHeight);
            bookDescriptionAvailableHeight = Math.round(lineHeight * numLines);
        } else {
            var bdOffsetTop = document.getElementById("bookDescription_feature_div").offsetTop;
            var imageBlockOffsetTop = document.getElementById("booksImageBlock_feature_div").offsetTop;
            var imageBlockHeight = document.getElementById("booksImageBlock_feature_div").offsetHeight;
            bookDescriptionAvailableHeight = imageBlockOffsetTop + imageBlockHeight -bdOffsetTop - 30;
            if(bookDescriptionAvailableHeight < minBookDescriptionInitialHeight) {
                bookDescriptionAvailableHeight = minBookDescriptionInitialHeight;
            }
        }
    		
    	var psTotalHeight = document.getElementById("postBodyPS").offsetHeight;
 
    	if(psTotalHeight > bookDescriptionAvailableHeight + 30){
    		if(document.getElementById("bdSeeLessPrompt").style.display == "none"){
    	    	document.getElementById("outer_postBodyPS").style.height = bookDescriptionAvailableHeight + 'px';
    	    	document.getElementById("psPlaceHolder").style.display ="block";
    	    	document.getElementById("bdSeeAllPrompt").style.display ="block";
    		}
    		else{
    			document.getElementById("outer_postBodyPS").style.height = psTotalHeight + 'px';
    		}
        }
        else{
        	document.getElementById("outer_postBodyPS").style.height = psTotalHeight + 'px';
        	document.getElementById("psPlaceHolder").style.display ="none";
        	document.getElementById("bdSeeAllPrompt").style.display ="block";
	    	document.getElementById("bdSeeLessPrompt").style.display ="none";
	    	document.getElementById("bdExpanderIcon").className = document.getElementById("bdExpanderIcon").className.replace("rotate","");
        }
    }
	    
	options.iframeId = iframeId;
	options.iframeWrapperId = "bookDesc_iframe_wrapper";
	options.overriddenCSSId = "bookDesc_override_CSS";
	options.encodedIframeContent = bookDescEncodedData;
	options.initialResizeCallback = resizeCallback;
	
	BookDescriptionIframe = new DynamicIframe(options);
	BookDescriptionIframe.createIframe();
	    
	if ((typeof BookDescriptionIframe != 'undefined') && (BookDescriptionIframe instanceof DynamicIframe)) {
	  P.when('jQuery').execute(function($) {
	    $(window).resize(function() {
	        BookDescriptionIframe.resizeIframe(resizeCallback);
	    });
	    $(window).bind('imageResize', function() {
	    	BookDescriptionIframe.resizeIframe(resizeCallback);
	    });
	  });
	}
});
</script>


	                
                
            
        
        
        
        
        
        
    
    
    

    















