    });
  },1000)
  $("._kt_mob_acc_header").on("click",function(){
    var contID = $(this).attr("data-content");
    if(!$(contID).is(":visible")){
        $(contID).slideDown(450);
        $(this).addClass('open');

    }else{
        $(contID).slideUp(450);
        $(this).removeClass('open');
    }
  });


