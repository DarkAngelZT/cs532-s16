                  <div id="article_inset">
                    <div id="story-package" >
                      <!-- series -->




<div id="series">
<script type="text/javascript" src="http://media.masslive.com/storypackage/13092339/sp.js" /></script>
</div>



<!-- /series -->

 <!-- Aka Story Package -->
                      <!-- gallery-preview -->
