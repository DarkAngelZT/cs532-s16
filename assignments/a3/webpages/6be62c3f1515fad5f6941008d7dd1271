    <!-- Header Mixpanel Tracking -->
    
        <script type="text/javascript">
            (function ($) {
                var headerLink = $('header.wrapper-header a');

                headerLink.each(function() {
                    var linkElement = $(this)[0];
                    var funct = "addEventListener";
                    if (!linkElement.addEventListener && !!linkElement.attachEvent){
                        //Polyfill for IE8
                        funct = "attachEvent";
                    }

                    linkElement[funct]('click', trackHeaderClick, false);
                });

                function trackHeaderClick () {
                    $this = $(this),
                    targetLink = $this[0].pathname,
                    currentPage = window.location.pathname;

                    mpq.push(["track", "Click: Header - " + targetLink ,
                       {
                           "token": mpqMasterPrj,
                           "!page": currentPage
                       }
                   ]);
                }

        })(jQuery);
        </script>
    

