    <script>
    jQuery(document).ready(function($){
        $.getJSON('//api.chartbeat.com/live/toppages/v3/', {
            apikey: 'a4503fe8959907b760985404440e62c6',
            host: 'host.madison.com',
            
            section: "/sports",
            
            limit: '20'
        }).done(function(oData){
            var i = 0;
            $.each( oData.pages, function(iDx, oItem){
                if(/_/i.test(oItem.path)){
                    i++;
                    var sTrendingItem = '<li class="chartbeat-trending-item"><h4><a href="http://'+oItem.path+'">'+oItem.title+'</a></h4></li>';
                    $('#chartbeat-trending-list-1072104').append(sTrendingItem);
                    if(i === 5) return false;
                }
            });
            $('#chartbeat-trending-list-1072104').fadeIn();
        }).fail(function(){
            $('#chartbeat-trending-list-1072104').before('<div class=\"alert alert-danger\">\n            <ul>\n                \n                    <li>An Error Occured</li>\n                \n            </ul>\n        </div>');
        }).always(function(){
            $('#chartbeat-trending-loading-1072104').fadeOut('fast');
        });
    });
