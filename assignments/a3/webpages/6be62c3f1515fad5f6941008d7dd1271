        <script type="text/javascript">
            // check to see if nav timing is supported by the web browser
            if (('performance' in window) && ('timing' in window.performance) && ('navigation' in window.performance)) {

                window.onload = function () {

                    setTimeout(function () {
                        var performance = window.performance;
                        var t = performance.timing;
                        var mpq = window.mpq;
                        var navTimingObj = {
                            '!REDIRECT_TIME':           t.redirectEnd - t.redirectStart, // Time consumed preparing the new page
                            '!DNS_TIME':                t.domainLookupEnd - t.domainLookupStart, // DNS query time
                            '!TCP_TIME':                t.connectEnd - t.connectStart, // TCP connection time
                            '!DOM_TREE_COMPLETE':       t.domComplete - t.domInteractive, // Time spent constructing the DOM tree
                            '!DOM_INTERACTIVE':         t.domInteractive - t.responseEnd, // Request to completion of the DOM loading
                            '!REQUEST_TIME':            t.responseEnd - t.requestStart, // Time spent during the request
                            '!LOAD_EVENT_TIME':         t.loadEventEnd - t.loadEventStart, // Load event time
                            '!TOTAL_LOAD_TIME':         t.loadEventEnd - t.fetchStart // Total time from start to load
                        };

                        if (mpq) {
                            mpq.push(["track", "Perf: Nav Timing Full", $.extend({
                                'token':                window.mpqMasterPrj,
                                '!PACKTAG_DISABLED':    'false',
                                '!HOSTNAME':            window.location.hostname
                            }, navTimingObj)]);
                        }

                    }, 0);
                }

            }

        </script>

    </body>
</html>


