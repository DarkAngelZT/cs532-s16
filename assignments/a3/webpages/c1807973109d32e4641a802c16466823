		$(document).ready(function(){
			$(".proNoti").click(function(){
				var X=$(this).attr('id');			
				if(X==1){
					$(".myDropdownMenu").hide();
					$(this).attr('id', '0');	
				}
				else{	
					$(".myDropdownMenu").show();
					$(this).attr('id', '1');
				}
		
			});	
			//Mouseup textarea false
			$(".myDropdownMenu").mouseup(function(){
				return false
			});
			$(".proNoti").mouseup(function(){
				return false
			});
		
			//Textarea without editing.
			$(document).mouseup(function(){
				$(".myDropdownMenu").hide();
				$(".proNoti").attr('id', '');
			});
		});
	</script>	
