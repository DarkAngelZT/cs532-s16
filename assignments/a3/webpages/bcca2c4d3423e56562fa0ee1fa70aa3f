<script>
// Our callback function, for when a feed is loaded.
function feedLoaded(result) {
if (!result.error) {
// Grab the container we will put the results into
var container = document.getElementById("linkarea");
container.innerHTML = '';
// Loop through the feeds, putting the titles onto the page.
// Check out the result object for a list of properties returned in each entry.
// http://code.google.com/apis/ajaxfeeds/documentation/reference.html#JSON
entries = result.xmlDocument.getElementsByTagName('item');
storylist = []
var links = document.getElementsByTagName('link');
for (var x=0,y=links.length; x<y; x++) {
if (links[x].rel.toLowerCase() == "canonical") {
canonical = links[x];
}
}

for (var i = 0; i < entries.length; i++) {
var div = document.createElement("div");
var title = entries[i].getElementsByTagName('title')[0].childNodes[0].nodeValue;
var link = entries[i].getElementsByTagName('link')[0].childNodes[0].nodeValue;

//var img = entries[i].getElementsByTagName('enclosure')[0].getAttribute('url');


if (!entries[i].getElementsByTagName('enclosure')[0]){ var theimg = ''}
else {
    var theimg = entries[i].getElementsByTagName('enclosure')[0].getAttribute('url');;
    if (canonical.href == link){}
    else { storylist.push([title, link, theimg]) }
    }
}
for (var i = 0; i <storylist.length; i++){
if (i < 2){
$('<div class="brief"></div>').html('<a href="' + storylist[i][1] + '"><img class="previewImage" height="70px" src="' + storylist[i][2] +'"></img><p>' + storylist[i][0] + '</p></a>').appendTo('#linkarea');
}
if (i == 2){
$('<div class="brief" style="border-right:0px; padding-right:15px"></div>').html('<a href="' + storylist[2][1] + '"><img class="previewImage" height="70px" src="' + storylist[2][2] +'"></img><p>' + storylist[2][0] + '</p></a>').appendTo('#linkarea');
$('<div style="clear:both"></div>').appendTo('#linkarea');
}


}
}
}
function OnLoad() {
// Create a feed instance that will grab Digg's feed.
var feed = new google.feeds.Feed("http://www.krcrtv.com/bmg/servlet/page/view/bmg-cali/-/14285892/14774432/-/format/rss_2.0/view/asFeed/-/cjvkauz/-/index.xml");
feed.setResultFormat(google.feeds.Feed.MIXED_FORMAT);
feed.setNumEntries(8)
// Calling load sends the request off. It requires a callback function.
feed.load(feedLoaded);
}
google.setOnLoadCallback(OnLoad)
</script> </section>
