function writeCookie(t,n,e){if(e){var o=new Date;o.setTime(o.getTime()+24*e*60*60*1e3);var s="; expires="+o.toGMTString()}else var s="";document.cookie=t+"="+n+s+"; domain=.sanook.com;path=/"}function getCookie(t){for(var n=t+"=",e=document.cookie.split(";"),o=0;o<e.length;o++){for(var s=e[o];" "==s.charAt(0);)s=s.substring(1,s.length);if(0==s.indexOf(n))return s.substring(n.length,s.length)}return null}matchMedia("only screen and (min-width: 1025px)").matches&&$(document).ready(function(){$(".btn-show-rt2013>span, .btn-show-rt2013>div").mouseover(function(){$(this).next().children().fadeIn("fast")}),$(document).bind("mouseout",function(t){var n=$(t.target);n.parents().hasClass("btn-show-rt2013")||$('[class*="btn-show-rt2013"] [class*="sanook-menu"], [class*="btn-show-rt2013"]>div>[class="arrow"]').fadeOut("fast")})}),$("body>footer>div>a").click(function(t){t.preventDefault(),$(window).scrollTop(120)}),$(document).ready(function(){$(window).scroll(function(){$(this).scrollTop()>800?$('[title="ขึ้นด้านบน"]').fadeIn():$('[title="ขึ้นด้านบน"]').fadeOut()}),$('[title="ขึ้นด้านบน"]').click(function(){return $("html, body").animate({scrollTop:0},1e3),!1}),"function"==typeof matchMedia&&matchMedia("only screen and (min-width: 1025px)").matches&&($(".btn-show-rt2013>span").mouseover(function(){$(this).next().children().fadeIn("fast")}),$(".btn-show-rt2013 > div > .sanook-menu").bind("mouseleave",function(t){$('[class*="btn-show-rt2013"] [class*="sanook-menu"], [class*="btn-show-rt2013"]>div>[class="arrow"]').fadeOut("fast")}))});
