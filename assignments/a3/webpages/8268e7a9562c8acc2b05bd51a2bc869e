<script>$(function() {
	var scroll = $('.switch-articles').jScrollPane({
		mouseWheelSpeed: 60,
		verticalDragMaxHeight : 150,
		contentWidth: '0px'
	});
		$(document).on('click','.side-col',function(){
		var type = $(this).attr('data-type');
		var currentImg = $(this).find('img').attr('src');
		$(this).find('img').attr('src','/images/miniBtns/loading.gif');
		$('.side-col').removeClass('related-selected');
		$.ajax({
			url: '/articles/a_relatedCore.php',
			type: 'POST',
			context: this,
			data: {type:type,where:'menu'},
			success: function(data){
				$(this).addClass('related-selected');
				$(this).find('img').attr('src',currentImg);
				$('.related-wrapper').html(data);
				var scroll = $('.switch-articles').jScrollPane({
					mouseWheelSpeed: 20,
					verticalDragMaxHeight : 150,
					contentWidth: '0px'
				});
			}
		});
	});
});
</script>
