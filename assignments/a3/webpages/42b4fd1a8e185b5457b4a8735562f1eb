  <script type="text/javascript">
        $(function() {

            $('#sort-switch button').click(function() {
                var sort = $(this).attr('value');

                $('.search-sort-selector-field').val(sort);
            });

            $('#helpicon').click(function() {
            	if (helpPopoverVisible) {
                         $('#search-help-popover').hide();
                         helpPopoverVisible = false;
                } else {
	                showHelpPopover();
                }
            });

        	var $searchSort = $('#search-sort');
	        var $searchSortLinks = $('.search-sort-links li');
			$searchSortLinks.each(function (i, elem) {
		        if ($searchSort.val().indexOf($(this).attr("data-search-sort")) !== -1) {
	    	        $(this).addClass("active");
	        	}
	    	});

		    $searchSortLinks.on("click", function () {
		        if (!$(this).hasClass("disabled")) {
		            $searchSortLinks.removeClass("active");
		            $(this).addClass("active");
		            $searchSort.val($(this).attr("data-search-sort"));
		            $("#search-query").val($("#search-query").attr('placeholder'))
		            $("#search-query-form").submit();
		        }
		    });

            var questionsViewUrl = "/questions/{id}/{plug}.html";

            var popoverVisible = false;
            var helpPopoverVisible = false;

            var inFocus = false;
            var inPopover = false;
            var inNavbar = false;

            function isHidable() {
                return !(inFocus || inNavbar || inPopover);
            }

            function showPopover() {
                if (!popoverVisible) {
                    var field_pos = $('#search-query').offset();

                    $('#search-options-popover').css({
                        top: (field_pos.top - $(window).scrollTop() + 35) + 'px',
                        left: (field_pos.left - 320) + 'px'
                    });

                    $('#search-options-popover').show();
                    popoverVisible = true;
                }
            }

            function showHelpPopover() {
                 if (!helpPopoverVisible) {
                     var field_pos = $('#search-query').offset();

                     $('#search-help-popover').css({
                         top: (field_pos.top - $(window).scrollTop() + 35) + 'px',
                         left: (field_pos.left - 750) + 'px'
                     });

                     $('#search-help-popover').show();
                     helpPopoverVisible = true;
                 }
             }

            function setToHide() {
                if (isHidable()) {
                    window.setTimeout(function() {
                        if (isHidable()) {
                            if (popoverVisible) {
                                $('#search-options-popover').hide();
                                popoverVisible = false;
                            }
                            if (helpPopoverVisible) {
                                 $('#search-help-popover').hide();
                                 helpPopoverVisible = false;
                            }
                        }
                    }, 750);
                }
            }

            $('.navbar-fixed-top').mouseenter(function() {
                inNavbar = true;
            }).mouseleave(function() {
                inNavbar = false;
                setToHide();
            });


            var currentQuery = '';
            var queryCache = {};

            $('#search-query').focus(function() {

                var $this = $(this);
                inFocus = true;
                showPopover();

            }).blur(function() {
                inFocus = false;
                setToHide();
            }).keyup(function() {
               var currentVal = $('#search-query').val();

               window.setTimeout(function() {
                   if (popoverVisible && currentVal == $('#search-query').val()) {
                       doSearch();
                   }
               }, 400);
            }).blur();

            var currentSearch = false;

            function doSearch() {

                var $input = $('#search-query');
                var $cont = $('#search-options-popover').find('.similar-questions-container');
                var lang = $('.search-lang-selector-field').val();
                var $results = $cont.find('.similar-questions-results');
                var ajaxLoaderImg = "/themes/base/admin/img/ajax-loader.gif";

                function showSpinner() {
                    $input.css("background-image", "url('" + ajaxLoaderImg + "')");
                }

                function hideSpinner() {
                    $input.css("background-image","none");
                }

                function processResults(data) {

                    //queryCache[data.searchResults.query] = data;

                    if (!data.searchResults.query == $input.val()) {
                        return;
                    }

                    if (data.searchResults.count > 0) {

                        currentSearch = false;

                        $cont.show();
                        $results.html('');

                        $.each(data.searchResults.results, function(i, node) {
                            var url;

                            if (node.type == 'kbentry') {
                                //url = kbEntryViewUrl;
                            } else if (node.type == 'idea') {
                                url = ideaViewUrl.replace("{plug}", node.plug);

                            } else {
                                url = questionsViewUrl.replace("{plug}", node.plug);
                            }

                            url = url.replace("{id}", node.id);

                            var title;

                            if (node.title) {
                                title = node.title;
                            } else {
                                title = node.name;
                            }

                            var type = node.type;

                            if (node.type == 'kbentry') {
                                type = 'doc';
                            } else if (node.type == 'topic') {
                                type = 'topic';
                            }

                            var the_date = node.creationDateFormatted;

                            $results.append('<p class="search-options-result"><span class="search-options-result-type ' + node.type + '">' + type +
                                    '</span><a href="' + url + '">' + title + ', ' + the_date + ' </a></p>');
                        });
                    } else {
                        $results.html('');
                        $cont.hide();
                    }
                }

                if ($input.val()) {

                    if (false && queryCache[$input.val()] != undefined) {
                        processResults(queryCache[$input.val()]);
                    } else {
                        var data = commandUtils.extractFormData($('#search-query-form'));
                        //var data = {q: $input.val()};
                        data.q = data.q + "*";

                        data.lang = lang;
                        data['pageSize'] = 10;

                        if (currentSearch) {
                            currentSearch.abort();
                        }

                        currentSearch = $.ajax({
	                        dataType: "json",
	                        url: '/search.json',
	                        data: data,
	                        beforeSend: showSpinner,
	                        success: processResults,
	                        complete: hideSpinner
	                    });
                    }

                } else {
                    $results.html('');
                    $cont.hide();
                }

            }

            $('#search-options-popover').mouseenter(function() {
                inPopover = true;
            }).mouseleave(function() {
                inPopover = false;
                setToHide();
            }).find('.search-query-submit').click(function() {
                $('#search-query-form').submit();
                return false;
            });

            function getURLParameter(name) {
                return decodeURI(
                        (RegExp(name + '=' + '(.+?)(&|$)').exec(location.search)||[,null])[1]
                );
            }

            function checkDefaults() {

                function enableInput(value, docs) {
                    $("input[name=search-container][value=" + value + "][data-isdocs" + ( docs? "" : "!") + "=true]").attr("checked", "checked");
                }

                var c = getURLParameter('c');
                var type = getURLParameter('type');
                var lang = getURLParameter('lang');

                if (c == 7) {
                    enableInput("all");
                }
            }

            function changeForm() {
                var $sel = $('#search-options-popover input[name=search-container]:checked');
                var val = $sel.val();

                if (val != 'all') {
                	//FIXME : Say what ?
                    //$('#search-query-form input[name=c]').val($sel.val());

                    $('#search-query-form input[name=type]').val('question OR idea');

                } else if (val == 'all') {
                    $('#search-query-form input[name=c]').val(7);
                    $('#search-query-form input[name=type]').val('question OR idea');
                } else {
                    $('#search-query-form input[name=type]').val('question OR idea');
                }

                doSearch();
            }

            $('#search-options-popover input[name=search-container]').change(changeForm);
            $('.search-lang-selector button').not('.sort-selector').click(function() {
                $('.search-lang-selector-field').val($(this).attr('value'));
                doSearch();
            });

            checkDefaults();
            changeForm();
        });

    </script> 
