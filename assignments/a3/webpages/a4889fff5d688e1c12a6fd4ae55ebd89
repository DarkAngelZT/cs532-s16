				<script>
					$(document).mouseup(function (e)
					{
						var container = $("#login");
					
						if (!container.is(e.target) // if the target of the click isnt the container...
								&& container.has(e.target).length === 0) // ... nor a descendant of the container
						{
							if ($("#login-trigger").hasClass("active")){
								$("#login-content").slideToggle();
								$("#login-trigger").toggleClass("active");
								$("#login-trigger").find("span").html("&#x25BC;");
							}
						}
						if (!$("#searchForm").is(e.target) // if the target of the click isnt the container...
								&& $("#searchForm").has(e.target).length === 0) // ... nor a descendant of the container
						{
							$("#searchFormText").val("");
						}
					});
				</script>
						
