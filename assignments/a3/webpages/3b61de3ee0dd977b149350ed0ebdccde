                var adsGlobalObj = [];

                function fireC2Backfill(event) {
                    if (YMedia && event["data"] !== undefined) {
                        var C2Event;

                        // if event.data is not an object, try to parse it first
                        try {
                            if (event.data instanceof Object == false && YMedia.JSON && YMedia.JSON instanceof Object) {
                                C2Event = YMedia.JSON.parse(event.data);
                            }
                        } catch(e) {}

                        if (adsGlobalObj instanceof Array && C2Event !== undefined && C2Event["msg"] !== undefined) {
                            if ((C2Event.msg == "rmx:no-ad" || C2Event.msg == "sidekicktv:no-ad") && C2Event.ad !== "" && adsGlobalObj[C2Event.ad] !== undefined && adsGlobalObj[C2Event.ad].selector !== undefined) {
                                var backfillPos = adsGlobalObj[C2Event.ad],
                                        backfillContainer = YMedia.one(backfillPos.selector);

                                if (!backfillContainer) {
                                    return;
                                }

                                if (C2Event.msg == "sidekicktv:no-ad" ) {
                                    YMedia.Global.fire("rapid-tracker:event", {
                                        mod_id: "sidekicktv",
                                        data: {
                                            name: "secview",
                                                keys: {
                                                sec: "stv",
                                                slk: "postmsg"
                                            }
                                        }
                                    });
                                    if (typeof SidekickTV !== "undefined") {
                                        backfillContainer.setStyle("display", "none");
                                        if (typeof sidekickTVBackFill !== "undefined") { 
                                            sidekickTVBackFill();
                                        } else {
                                            YMedia.Global.fire("sidekicktv:backfill");
                                        }
                                    } else {
                                        YMedia.Global.fire("rapid-tracker:event", {
                                            mod_id: "sidekicktv",
                                            data: {
                                                name: "secview",
                                                    keys: {
                                                    sec: "stv",
                                                    slk: "notfound"
                                                }
                                            }
                                        });
                                    }
                                } else {
                                    backfillContainer.setStyle("display", "none");
                                    if (typeof SidekickAdsHandlerInstance !== "undefined") {
                                        YMedia.Global.fire("sidekick:backfill");
                                    } else {
                                        backfillContainer.setAttribute("data-ad-backfill", "4");
                                        YMedia.Global.fire("ads:enhance", YMedia.one(".yog-rp"));
                                    }
                                }
                            }
                        }
                    }
                }

                if (window["addEventListener"] !== undefined && window["postMessage"] !== undefined) {
                    window.addEventListener("load", fireC1Backfill, false);
                    window.addEventListener("message", fireC2Backfill, false);
                } else {
