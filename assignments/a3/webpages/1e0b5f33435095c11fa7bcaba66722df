			                Top: { name: "Top", width: 728, height: 90 },
			                Middle1: { name: "Middle1", width: 300, height: 250 },
			                Middle2: { name: "Middle2", width: 300, height: 250 },
			                Middle3: { name: "Middle3", width: 300, height: 250 }
			            }
			        },
			        none: {
			            small: { },
			            large: { }
			        }
				};
				var oas_tag = {};
			    oas_tag.allowSizeOverride = true;
			    var cbc_page_type = "comicbook.com/";
			    if (cbc_page_stamp == "home") {
			        			        cbc_page_type = cbc_page_type + "Home";
			        			    } else if (cbc_page_stamp == "topiclist") {
			        cbc_page_type = cbc_page_type + "Boards";
			    } else if (cbc_page_stamp == "category" || cbc_page_stamp == "listings") {
			        cbc_page_type = cbc_page_type + "Categories" + "";
			    } else {
			        cbc_page_type = cbc_page_type + "Articles";
			    }

				oas_tag.site_page = cbc_page_type;
				if (window.yieldbot != undefined) {
					try {
					    ybotq.push(function() {
					        oas_tag.query += "&ybot=" + yieldbot.getPageCriteria('_', '/'); //getPageCriteria(innerDelimiter,outerDelimiter)
					    });
					} catch (e) { //may want to output an error message to the console
					}
				}

				ybotq.push(function() {
				    oas_tag.url = 'oas.comicbook.com';
					oas_tag.sizes = function () {
				        for(var key in cbc_sizes[cbc_page_stamp][cbc_screen_size]) {
				            if ($('#oas_' + cbc_sizes[cbc_page_stamp][cbc_screen_size][key]["name"]).length) {
				                if ($('#oas_' + cbc_sizes[cbc_page_stamp][cbc_screen_size][key]["name"]).hasClass("hidden")) {
				                    $('#oas_' + cbc_sizes[cbc_page_stamp][cbc_screen_size][key]["name"]).removeClass('hidden');
				                }
				                if (cbc_page_stamp in cbc_scroll_ads) {
				                    $('.' + cbc_scroll_ads[cbc_page_stamp][cbc_screen_size]["tag"]).each(function() {
				                        var oasTag = $(this);
				                        oasTag.attr("id", "oas_" + cbc_scroll_ads[cbc_page_stamp][cbc_screen_size]["tag"]);
				                        if (oasTag.hasClass("hidden")) {
				                            oasTag.removeClass("hidden");
				                        }
				                    });
				                }
				                oas_tag.definePos(cbc_sizes[cbc_page_stamp][cbc_screen_size][key]["name"], [cbc_sizes[cbc_page_stamp][cbc_screen_size][key]["width"],cbc_sizes[cbc_page_stamp][cbc_screen_size][key]["height"]]);
			            	}
			        	}
					};
				    oas_tag.callbackHandler = function () {
				        oas_tag.addHandler('callbackId', function(data){
				            $global.ReplaceAd();
				        })
				    };

					oas_tag.site_page = cbc_page_type;

			        if (typeof document.referrer !== 'undefined' && document.referrer != '') {
			            var referralDomain = "";
						if (document.referrer.match(/^https?:\/\/([^\/]+\.)?facebook\.com(\/|$)/i)) {
			            	referralDomain = "facebook";
						} else {
			            	var referrer = document.referrer.match(/:\/\/([^\/:]+)(:\d+)?\//)[1];
							if (referrer != "localhost") {
			                	referralDomain = referrer.match(/.*?([^.]+)[.][^.]+$/)[1];
			            	} else {
			                	referralDomain = referrer;
			            	}
			          	}
			        }

				    if ($('[data-tags]').length) {
						var keywords = $('[data-tags]').data('tags');
					}

			        oas_tag.query = 'status=' + $cbc.User.Status + (typeof document.referrer !== 'undefined' ? ('&refdn=' + referralDomain) : '') + '&Newsletter=' + $cbc.Guest.Newsletter + ($('[data-tags]').length ? ('&keywords=' + keywords) : '' + "&ybot=" + yieldbot.getPageCriteria('_', '/'));

					(function() {
						oas_tag.version ='1';
						var oas = document.createElement('script'),
							protocol = 'https:' == document.location.protocol?'https://':'http://',
							node = document.getElementsByTagName('script')[0];
							oas.type = 'text/javascript';
							oas.async = true;
							oas.src = protocol + oas_tag.url + '/om/' + oas_tag.version + '.js';
							node.parentNode.insertBefore(oas, node);
					})();
				});
			</script>
