    $('#password-reset-open-page-manager-pop').click(function(){
        $("#tncms-password-reset-form-page-manager-pop").show();
        $("#tncms-login-form-page-manager-pop").hide();
    });
// switch back to login form
    $('#password-reset-close-page-manager-pop').click(function(){
        $("#tncms-login-form-page-manager-pop").show();
        $("#tncms-password-reset-form-page-manager-pop").hide();
    });
