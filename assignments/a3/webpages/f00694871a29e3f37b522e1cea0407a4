<div id="event_results_wrapper">

    

             <script type="text/javascript">
        $(document).ready(function() {

            //get total events for results/page functionality
            totalEvents = GetTotalEvents() ? parseInt(GetTotalEvents()) : $('.event').length - 1;
            //load page
            //if (HasRecordedPage() && !ajaxLoaded) {
		//ajaxLoaded = true;
                //ApplyPage();
            //}

            //There are now event rows coming in that are already loaded, hook up the appropriate events on these as well
            $('#tn_evt_results_area_main .view_tix.alreadyLoaded').find('a.empty_event').each(function (intIndex) {
                var parentTC = $(this).parents('.view_tix.alreadyLoaded');
                var eventId = (parentTC.attr('eventid') != undefined) ? parentTC.attr('eventid') : '0';
                EventSubscriptionAlertMe($(this), eventId);
            });

            //take care of button loading
            LoadEventGridPricing();

            //bind reloading to events
            $('#event_results_wrapper .pager a').click(function() {
                ReloadEventResults($(this).attr('page'), GetCurrentSort(), GetCurrentSortDirection(), GetCurrentCount());
                return false;
            });
            $('#event_results_wrapper .sort-wrapper').click(function() {
                ToggleSortDirection($(this));
                TrackTableSort($(this).attr('sort'), $(this).attr('sortdir'));
                ReloadEventResults(GetCurrentPage(), $(this).attr('sort'), $(this).attr('sortdir'), GetCurrentCount());
                return false;
            });
          
            $('.results-count').each(function() {
                var thisCount = parseFloat($(this).attr('evtsperpage'));
                var isCurrentCount = thisCount.toString() == GetCurrentCount();
                if(totalEvents / thisCount > .5 && totalEvents > 25) {
                    if(!isCurrentCount) {
                        var hrefUrl;
                        var pageQs = GetPageQueryString();
                        var countStr = pageQs.match(/count=\d*/i);
                        hrefUrl = (pageQs.indexOf('count') > -1) ? pageQs.replace(countStr, 'count=' + thisCount) : pageQs + 'count=' + thisCount;
                        $(this).attr('href', hrefUrl);

                        $(this).click(function() {
                            TrackPagination($(this).attr('evtsperpage'), $(this));
                            ReloadEventResults(GetCurrentPage(), GetCurrentSort(), GetCurrentSortDirection(), $(this).attr('evtsperpage'));
                            return false;
                        });
                    } else {
                        $(this).addClass('count_active');
                        $(this).removeAttr('href');
                    }
                } else {
              if(isCurrentCount) {
                  $(this).addClass('count_active');
              }
                $(this).addClass('inactive');
                $(this).removeAttr('href');
            }
            });
        });
        </script><div style="display:none;" id="no_tickets_left_content"><span class="no_tickets_left_text">No Tickets Available</span></div><div id="results_count_wrapper" count="25"><span class="results-count-label">Results per page:</span>

